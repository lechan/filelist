<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0,maximum-scale=1" />
	<title>榜单</title>	
	<link rel="stylesheet" type="text/css" href="css/base.css">
	<link rel="stylesheet" type="text/css" href="css/toplist.css">
	<style>
.nav-ctn{
	height:300px;
}
.nav-ctn-inner > *{
	font-size:30px;
	text-align:center;
}
.nav-ctn-inner > *:nth-child(even){
	background:#eee;
}

	</style>
</head>
<body>
<div class='nav-justified-outer' id='header'>
	<div class='nav-justified-ctnr'>
		<ul id='navList' class='nav nav-justified'>
			<li><a href='javascript:void(0)'>1</a></li>
			<li><a href='javascript:void(0)'>2</a></li>
			<li><a href='javascript:void(0)'>3</a></li>
			<li><a href='javascript:void(0)'>4</a></li>
			<li><a href='javascript:void(0)'>5</a></li>
		</ul>
	</div>
</div>
<div id='navListCtnr' class='nav-ctn'>
	<div>1</div>
	<div>2</div>
	<div>3</div>
	<div>4</div>
	<div>5</div>
</div>
<script type="text/javascript" src='js/zepto.js'></script>
<script type="text/javascript" src='js/video.js'></script>
	<script>
		$(function(){
			var btns=$('#navList').children(), parts=$('#navListCtnr').children();
			var navData=[];
			$.each(btns, function(ix, el){
				navData.push({btn:$(btns[ix]), ctn:$(parts[ix]), loaded:true});
			});
			new LENOVO.util.Nav(navData);
		});
	</script>
</body>
</html>


