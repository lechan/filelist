#slider{
	overflow:hidden;
}

.ctnr{
	padding:14px 18px;
	background-color: rgb(238, 238, 238);
}
.box{
	display:inline-block;
	vertical-align:top;
}
.tpl{
	display:none !important;
}
.empty:after {
	content: '无数据' !important;
	display:block !important;
	line-height:2em;
	text-align:center;
	font-size: 1rem;
}
.blk{
	background-color:white;
	box-shadow:0px 2px 4px #ccc;
	margin-bottom:16px;
	display:block;
}
.btn{
	display: inline-block;
	text-align: center;
}
.btn1{
	border:2px solid #fe8800;
	border-radius:30px;
	height:54px;
	line-height: 50px;
	color:#fe8800;
	display:inline-block;
	padding: 0 1em;
	text-align: center;
}
.multi-clm-box > *{
	float:left;
}
.multi-clm-box > *:last-child{
	float:none;
	overflow:hidden;
}
.multi-clm-box:after{
	content:'';
	clear:both;
	display: block;
}
.multi-clm-box2{
	font-size:0;
	white-space: nowrap;
}
.multi-clm-box2 > *{
	display:inline-block;
	vertical-align:top;
	font-size:1rem;
}

#toast {
	position: fixed;
	font-weight: bold;
	z-index: 999;
	padding: 10px;
	color: #fff;
	font-size: 16px;
	background-color: #000;
	opacity: .8;
	border-radius: 6px;
	box-shadow: 0px 1px 5px 0px #000;
	bottom: 75px;
}

.swipe-wrap { overflow: hidden; height: 100%;}
.swipe-wrap > li { float: left; width: 100%; height: 100%; }
.swipe-wrap > li > a:active,
.swipe-wrap > li > a:focus{
	background:none;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
	tap-highlight-color: rgba(0, 0, 0, 0);
}
.swipe-wrap img{
	height:auto;
}
.ui-slider-dots{
	z-index: 100;
	position: absolute;
	bottom: 20px;
	width: 100%;
	text-align: center;
	font-size: 0;
}
.ui-slider-dots b {display:inline-block; font-size:0; width:20px; height:6px; margin-right:6px; background-color: #fff; }
.ui-slider-dots b.ui-slider-dot-select { background-color: #fe8800; }



.nav{
	font-size:0;
}
.nav a{
	display:inline-block;
	width:100%;
	height:100%;
	color:#757575;
}
.nav-justified-outer{
	padding:0 30px;
	border-bottom: 2px solid #c3c3c3;
}
.nav-justified{
	text-align: center;
	/*height:62px; useless for table, line-height takes effect*/
	line-height:61px;
	font-size:0px;
	white-space:nowrap;
	display:table;
	width:100%;
	overflow:hidden;
	table-layout:fixed; /* for equal spacing */
}
.nav-justified > li{
	display:table-cell;
	font-size:28px;
	background-color: white;
}
.nav-justified > li.active a{
	color:#fe8800;
}
.navbar{
	position:absolute;
	bottom:0;
	background:#fe8800;
	height:4px;
	left: 0;
}
.nav-ctn{
	width: 100%;
	overflow: hidden;
}
.nav-ctn-inner{
	height:100%;
	white-space:nowrap;
	font-size:0;
}
.nav-ctn-inner > *{
	white-space: normal;
	font-size:1rem;
	width: 100%;
	height:100%;
	overflow:auto;
	display:inline-block;
	vertical-align:top;
}
/**************************/

.appStars{ 
	display: inline-block;
}
.appStars:before{ 
	display: inline-block;
	content:'\2605\2605\2605\2605\2605';
	color:rgb(223, 223, 223);
}
.appStars:after{ 
	color:rgb(242, 222, 101);
	position:absolute;
	left:0;
	top:0;
}
.star1:after{ 
	content:'\2605';
}
.star2:after{ 
	content:'\2605\2605';
}
.star3:after{ 
	content:'\2605\2605\2605';
}
.star4:after{ 
	content:'\2605\2605\2605\2605';
}
.star5:after{ 
	content:'\2605\2605\2605\2605\2605';
}
/***********************/

.error-box{
	position:fixed;
	left:32px;
	top:20px;
	right:32px;
	bottom:20px;
}
.error-box form{
	position:absolute;
	left:0;
	top:0;
	right:0;
	bottom:110px;
	padding-bottom:2em;
	border: 0;
}
.error-box form textarea{
	width:100%;
	height:100%;
}
.error-box .err-btm{
	position:absolute;
	bottom:30px;
	width:100%;
	height:2em;
	text-align: right;
}

.review-box form{
	top:55px;
}
.review-box .star-box{
	text-align:center;
}
.review-box .appStars{
	font-size: 1.8em;
	margin-bottom: 0.2em;
}
/**********************/
