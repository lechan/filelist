html, body{
	padding:0;
	margin:0;
	font-size:24px;
}
*{
	position: relative;
	box-sizing: border-box;
}
ul, li{
	margin:0;
	padding:0;
	list-style:none;
}
button{
	background: transparent;
	outline: none;
	cursor: pointer;
	font-size:1rem;
	border: 0;
	margin: 0;
	padding: 0;
}
img{
	width:100%;
	height:100%;
}

a{
	text-decoration:none;
}
a:active, button:active{
	background-color:#f5f5f5;
}
.loading{
	min-height:60px;
}
.loading:before{
	z-index:99;
	content:'';
	position:absolute;
	width:40px;
	height:40px;
	left:50%;
	top:50%;
	margin-left:-20px;
	margin-top:-20px;
	background:url(../img/loading.gif) no-repeat center center;
}

.sliding{
	-webkit-transition-duration:500ms;
	-webkit-transition-timing-function:ease-out;
	-webkit-transition-property:transform;
	transition-duration:500ms;
	transition-timing-function:ease-out;
	transition-property:transform;
}

.hidden{
	display:none;
}

.mask {
	position: fixed;
	display: none;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	z-index: 1999;
	background-color: rgba(0,0,0,0.7);
}
#dialogWrapper {
	width: 100%;
	z-index: 2500;
	position: fixed;
}
.dilogWrap {
background: #fff;
color: #191919;
width: 90%;
margin: 0 auto;
height: 100%;
}

.dialogCityTit {
height: 106px;
line-height: 106px;
text-align: center;
font-size: 40px;
border-bottom: 1px solid #b7b7b7;
}
.cityList > a {
	display: block;
	height: 86px;
	line-height: 86px;
	padding: 0 136px 0 40px;
	border-bottom: 1px solid #e8e8e8;
	font-size: 28px;
	color: #191919;
	position: relative;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}
.cityList > a.on{
color:#27d57d
}
.cityList > a.on:after {
content: '';
width: 36px;
height: 36px;
position: absolute;
right: 50px;
top: 25px;
		background:url(../img/tick.png) no-repeat center center;
	}

#g3Tit{
	font-size: 20px;
line-height: 2em;
text-align: center;
}
#g3Body{
	border-top: 1px solid #999;
border-bottom: 1px solid #999;
padding: 1em;
}
#g3Footer{
	padding: 1em 0;
	font-size:18px;
}
#g3Footer:after{
	content:'';
	position:absolute;
	height:1em;
	top:1em;
	left:50%;
	border-left:1px solid #999;
}
#g3Footer button{
	width:50%;
	font-size: 18px;
	line-height: 1em;
}
#g3Go{
	color:#fe8800;
}
