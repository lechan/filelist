<body>
    
    
<script>

   function GetQueryString(name) {
    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)","i");
    var r = window.location.search.substr(1).match(reg);
    if (r!=null) return (r[2]); return null;
}
   
    window.onload=function()
    {
        if( "fjtc" == GetQueryString("app_name") ){
            document.getElementById("fjtc").style.display = "block";
        }else if("bjfc" == GetQueryString("app_name")){
            document.getElementById("bjfc").style.display = "block";
        }
        else if("zgtcb"== GetQueryString("app_name")){
           document.getElementById("zgtcb").style.display = "block";
        }
    }
</script>
