.count_down_box{ width:100%; background:#FA5428; padding:20px 0px;}
.count_down_box img{ width:100%;}
.count_down_box div{ text-align:center; color:#fff; font-size:48px; font-weight:bold;}

.ending_banner,.ending_bottom{ width:100%; overflow:hidden; position:relative;}
.ending_banner img,.ending_bottom img{ vertical-align:top; width:100%;}
.ending_content{ background:#FFF899; padding:40px 20px;}
.ending_banner h1{ position:absolute; bottom:0px; right:0px; text-align:right; padding:20px; font-size:24px; color:#fff; font-weight:bold;}
.ending_banner h1 strong{ font-size:48px; color:#fdff5b; margin:0px 10px;}
.ending_content h2{ font-size:24px; line-height:40px; color:#fa5327; font-weight:bold;}
.ending_content h3{ font-size:24px; color:#ffffff; line-height:40px; font-weight:bold; border:2px dashed #fa5327; border-radius:20px; padding:3px; margin:20px 0px;}
.ending_content h3 span{ background:#fa5327; display:block; border-radius:20px; padding:15px;}
.ending_content h4{ background:#FDF8B2; padding:20px; font-size:16px; color:#e06140; line-height:40px; margin-top:20px;}

