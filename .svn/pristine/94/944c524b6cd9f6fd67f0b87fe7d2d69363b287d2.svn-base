.ms_box_r{ width:55%;}
.ms_box_b{ clear:both; text-align:center; padding-top:20px;}
.ms_box_b p{ margin-bottom:20px;}
.ms_box_b p input{ width:168px; height:52px; background:#fff; border:1px solid #B2B2B2; margin-right:20px; vertical-align:top; padding:0px 10px; font-size:44px;}
.ms_box_b p img{ vertical-align:top;}
.ms_box_b a,.ms_box_b span{ width:170px;}
.back_box{ position:absolute; top:25px; left:25px; z-index:2;}
