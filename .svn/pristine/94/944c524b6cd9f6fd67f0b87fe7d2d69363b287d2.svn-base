@charset "utf-8";
/* CSS Document */

a{ cursor:pointer;}
#toast{bottom:-10em; width:100%; font-size:1.5em; opacity:0; background:#fff; height:200px; line-height:200px; z-index:100;}
/*animation & transition*/
.toastAnimation{-webkit-animation:toast 3s linear 1;}
@-webkit-keyframes toast{
	0%{opacity:0;}
	30%{opacity:1;}
	70%{opacity:1;}
	100%{opacity:0;}
}
.wrap{ max-width:640px; margin:0 auto;}
.banner{ width:100%; position:relative;}
.banner_wrap{ width:100%; overflow:hidden;}
.banner_wrap img{ width:100%; vertical-align:top;}
.prize_btn{ position:absolute; top:15px; right:0px;}
.intro_btn{ font-size:24px; color:#fdff5b; position:absolute; right:20px; bottom:50px; text-decoration:underline; font-weight:bold;}
.marquee_wrap{ width:100%; height:50px; line-height:50px; position:relative; overflow:hidden; background:#FEFFA4; font-size:24px;}
#marquee{ width:auto; position:absolute; top:0px; left:0px; white-space:nowrap; height:50px; line-height:50px;}
.marquee_con{ float:left;}
.marquee_wrap span{ margin-right:20px; color:#191919;}

.scrollList_wrap{ width:100%; height:210px; line-height:30px; position:relative; overflow:hidden; background:#FEFFA4; font-size:24px;}
#scrollList{ width:100%; position:absolute; top:0px; left:0px; white-space:nowrap; height:auto; line-height:30px;}
.scrollList_wrap{ display:block;}
.scrollList_wrap span{ color:#191919; width:100%; display:block;}



.title{ text-align:center; width:100%; padding-top:40px; margin-bottom:40px;}
.btn{ height:54px; line-height:54px; text-align:center; padding:0px 20px; display:inline-block; background:#FA5428; color:#fff; font-size:30px; overflow:hidden; font-weight:bold;}
.btn_grey{ height:54px; line-height:54px; text-align:center; padding:0px 20px; display:inline-block; background:#DADADA; color:#757575; font-size:30px; overflow:hidden; font-weight:bold;}

.con1{ background:url(../images/icon1.png) no-repeat 0 0 #fff; background-size:100%; width:100%; overflow:hidden; padding-bottom:40px;}
.con1_l{ width:45%; text-align:right;}
.con1_l img{ margin-right:20px;}
.con1_r{ width:55%;}
.con1_r p{ width:90%;}
.con1_r p strong{ font-weight:bold;}
.con1_r .p1{ font-size:18px; color:#fa5327; margin-top:10px;}
.con1_r .p2{ font-size:42px; color:#fa5327; margin:5px 0px;}
.con1_r .p3{ font-size:20px; color:#191919;}
.con1_r .p3 strong{ color:#fa5327;}
.con1_r .p4{ font-size:18px; color:#191919; margin-top:20px;}
.con1_r .p5{ font-size:28px; color:#fa5327;}
.con1_b{ clear:both; text-align:center; padding-top:20px;}
.con1_b h2{ font-size:34px; color:#fa5327; font-weight:bold; text-align:center;}

.con2{ background:url(../images/icon2.png) no-repeat 0 0 #FFF9E6; background-size:100%; width:100%; overflow:hidden; padding-bottom:20px; position:relative;}
.con2_wrap{ width:90%; margin:0 auto; padding-top:220px;}
.con2_item{ width:100%; padding:25px 0px; border-bottom:1px dashed #C7BEA2;}
.con2_item dl{ overflow:hidden;}
.con2_item dl dt{ width:70%; float:left;}
.con2_item dl dt h2{ color:#fa5327; font-size:36px; line-height:50px; font-weight:bold;}
.con2_item dl dt h3{ color:#191919; font-size:28px; line-height:40px;}
.con2_item dl dt h4{ color:#757575; font-size:18px; line-height:30px;}
.con2_item dl dt h5{ color:#191919; font-size:24px; line-height:60px;}
.con2_item dl dd{ width:30%; float:right; text-align:right;}

.scratch_box{ width:100%; background:url(../images/repeat.png) repeat-x bottom #fff; padding:30px 0px; margin-bottom:20px;}
.scratch_box h2{ text-align:center;}
.scratch_box .canvas_wrap{ width:540px; height:100px; margin:20px auto 0px; display:block; position:relative; overflow:hidden;}
.scratch_box .canvas_wrap canvas{ width:540px; height:100px; position:relative; z-index:2;}
.scratch_box .canvas_wrap div{ text-align:center; width:540px; height:100px; line-height:100px; font-size:50px; color:#191919; font-weight:bolder; position:absolute; top:0px; left:0px; z-index:1; letter-spacing:20px;}
.scratch_box .scratch_show{ width:540px; height:100px; margin:20px auto 0px; display:block; position:relative; overflow:hidden; border:1px dashed #CFCFCF; line-height:100px; text-align:center; font-size:36px; color:#A8A8A8;}
.scratch_info{ overflow:hidden; line-height:50px;}
.scratch_info span{ font-size:28px; color:#191919;}
.scratch_info span strong{ font-size:36px; color:#fa5327; font-weight:bold;}
.scratch_info span input{ width:145px; height:52px; border:1px solid #B2AEA1; background:#fff; font-size:24px; padding:0px 10px; vertical-align:top;}
.scratch_info span a{ vertical-align:top;}
.scratch_info p{ clear:both; font-size:18px; color:#757575; padding-top:10px;}
.scratch_again{ text-align:center; margin-top:20px; display:none;}

.act_box{ width:100%; overflow:hidden; margin-top:20px;}
.act_box h2{ margin-bottom:20px;}
.tab_app{ width:100%; background:#fff; border-radius:10px; overflow:hidden; border-bottom:5px solid #D9D4C4;}
.tab_app_btn{ width:100%; overflow:hidden; display:-webkit-box;}
.tab_app_btn li{ height:80px; -webkit-box-flex:1; background:#E0481C; cursor:pointer; text-align:center; line-height:80px; color:#F6C8BB; font-weight:bold; font-size:30px;}
.tab_app_btn li.active{ background:url(../images/arrow.png) no-repeat bottom center #FA5428; color:#fff;}
.tab_app_con{ position:relative; height:500px;}
.tab_app_con div{ position:absolute; top:0px; left:0px; width:100%; text-align:center; display:none;}
.tab_app_con div p{ font-size:18px; color:#757575; text-align:left; width:90%; margin:20px auto 0px;}
.tab_app_con div h3{ font-size:28px; color:#191919; line-height:40px; width:90%; margin:0 auto; padding-top:20px; text-align:left;}
.tab_app_con div h3 strong{ color:#fa5327;}
.tab_app_con div textarea{ width:90%; display:block; margin:20px auto; padding:10px; height:220px; border:1px solid #B2B2B2; resize:none; overflow:auto; font-size:28px;}
.app_list{ padding:20px; overflow:hidden;}
.app_list li{ width:25%; float:left; display:inline; height:140px; padding-top:20px;}
.app_list li a{ width:80%; display:block; margin:0 auto;}
.app_list li a img{ display:block; vertical-align:top; width:90px; margin:0 auto;}
.app_list li a span{ width:110px; font-size:18px; height:30px; overflow:hidden; line-height:30px; color:#434343; margin:0 auto; display:block; text-align:center; white-space:nowrap; text-overflow:ellipsis; }
.app_list li a span.selected{ color:#999;}
.app_list li.active{ background:#EDEDED;}

.con3{ background:url(../images/icon3.png) no-repeat 0 0 #fff; background-size:100%; width:100%; overflow:hidden; padding-bottom:40px;}
.con3_wrap{ width:90%; margin:0 auto; text-align:center;}
.con3_wrap h3{ font-size:24px; color:#191919; text-align:left; margin-bottom:20px; line-height:40px;}

.contact{ width:100%; padding:40px 0px; background:#FFF9E6;}
.contact h2{ font-size:24px; color:#191919; line-height:40px; width:95%; margin:0 auto 20px;}
.contact ul{ display:-webkit-box;}
.contact ul li{ -webkit-box-flex:1; text-align:center;}
.contact ul li h3{ font-size:20px; color:#757575; margin-top:10px;}
.contact h4{ width:90%; border-bottom:1px solid #FFCFBD; position:relative; margin:100px auto 100px; height:0px;}
.contact h4 div{ background:url(../images/icon6.png) no-repeat center #FFF9E6; width:145px; height:70px; position:absolute; top:-35px; left:50%; margin-left:-72px;}

#mask{ width:100%; height:100%; z-index:100; top:0px; left:0px; background:#000; opacity:0.5;}
#dialogWrapper{ width:90%; z-index:200; left:0px; top:0px; background:#fff; overflow:hidden;}
.pop_wrap{ padding:20px;}
.pop_wrap h2{ font-size:36px; color:#191919; margin-bottom:20px; text-align:center;}
.pop_wrap h2 strong,.pop_wrap h3 strong{ color:#fa5327;}
.pop_wrap h3{ font-size:24px; color:#000000; width:85%; margin:0 auto; line-height:40px;}
.pop_wrap h4{ font-size:24px; color:#757575; margin-top:20px; width:85%; margin:0 auto;}
.pop_wrap h5{ font-size:30px; color:#191919; width:85%; margin:0 auto;}
.pop_wrap p{ font-size:24px; color:#757575; width:85%; margin:0 auto; line-height:30px; margin-bottom:20px;}
.pop_wrap p strong{ color:#fa5327;}
.btn_box{ text-align:center; margin-top:20px;}
.btn_box a,.btn_box span{ width:180px; margin:0px 20px;}
.winner_info{ border-top:1px solid #D5D5D5; width:85%; margin:0 auto; padding-top:10px;}
.winner_info p{ width:100%; margin-bottom:20px; clear:both; overflow:hidden;}
.winner_info p label{ width:32%; float:left;}
.winner_info p input{ width:60%; float:left; height:30px; border:1px solid #B2B2B2; background:#fff; padding:0px 5px;}
.winner_info p select{ margin-right:2px; height:30px; border:1px solid #B2B2B2; background:#fff;}
.winner_info p.select_type1 select{ width:32%;}
.winner_info p.select_type2 select{ width:65%;}
.winner_info p.address input{ width:90%;}

.header{ width:100%; position:relative; height:86px; background:#FA5428; line-height:86px;}
.header h1{ font-size:36px; text-align:center; color:#fff; width:100%; height:100%; position:absolute; top:0px; left:0px; font-weight:bold;}
.back_btn{ float:left; margin-left:20px; font-size:28px; color:#fff; padding-left:25px; background:url(../images/arrow2.png) no-repeat left center; position:relative; z-index:2;}
.content{ padding:30px;}
.content h2{ font-size:28px; color:#fa5327; line-height:35px; margin-top:20px;}
.content h3{ font-size:24px; color:#fa5327; line-height:35px;}
.content p{ font-size:20px; color:#191919; line-height:35px;}
.content p strong{ color:#fa5327;}
.img_wrap{ text-align:center; margin-top:20px; margin-bottom:20px;}
.img_wrap span{ font-size:20px; color:#424242; display:block; text-align:center;}

.tab_prize{ width:100%; background:#fff; padding-bottom:50px;}
.tab_prize_btn{ width:100%; overflow:hidden; display:-webkit-box;}
.tab_prize_btn li{ height:75px; -webkit-box-flex:1; background:#E7E7E7; cursor:pointer; text-align:center; line-height:75px; color:#191919; font-weight:bold; font-size:28px;}
.tab_prize_btn li.active{ background:#fff; color:#fa5327;}
.tab_prize_con{ position:relative; min-height:400px;}
.tab_prize_con > div{ position:absolute; top:0px; left:0px; width:100%; text-align:center; display:none;}
.prize_title{ padding:30px; border-bottom:1px solid #D3D3D3; line-height:40px;}
.prize_title h2{ font-size:24px; color:#fa5327; text-align:left;}
.prize_title h3{ font-size:24px; color:#191919;}
.prize_list{ padding:0px 30px;}
.prize_list li{ padding:30px 0px; border-bottom:1px solid #D3D3D3; overflow:hidden; clear:both; text-align:left; line-height:40px;}
.prize_list_l{ width:70%;}
.prize_list_l h2{ font-size:28px; color:#191919; font-weight:bold;}
.prize_list_l h3{ font-size:20px; color:#757575;}
.prize_list_l h3 em{ color:#fa5327;}
.prize_tips{ clear:both; text-align:right; font-size:20px; color:#fa5327;}
.prize_info{ padding:30px 30px 0px; text-align:left;}
.prize_info h2{ clear:both; overflow:hidden; font-size:28px; color:#fa5327; font-weight:bold; line-height:60px;}
.prize_info h3{ font-size:24px; color:#757575;}
.prize_info h4{ font-size:20px; color:#191919;}
.prize_none{ padding:50px 0px; text-align:center;}
.prize_none h2{ font-weight:bold; font-size:24px; color:#757575; margin-bottom:20px;}

.ms_box{ margin-top:40px;}
.ms_box_l{ width:45%; text-align:right;}
.ms_box_l img{ margin-right:20px;}
.ms_box_r h2{ font-size:24px; color:#757575;}
