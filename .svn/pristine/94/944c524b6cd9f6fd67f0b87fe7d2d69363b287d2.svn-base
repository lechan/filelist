.act_intro h3{ font-size:24px; color:#fa5327; margin-bottom:10px; line-height:40px;}
.line{ width:100%; height:0px; overflow:hidden; border-bottom:1px solid #CCCCCC; margin:20px 0px;}

.preview_banner{ width:100%;}
.preview_banner img{ width:100%; vertical-align:top;}
.preview_tips{ width:100%;}
.tips_top{ background:url(../images/repeat2.png) repeat-x #FA5428; height:8px;}
