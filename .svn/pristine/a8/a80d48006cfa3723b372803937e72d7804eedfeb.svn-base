@charset "utf-8";

/* CSS reset */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fiedlset,input,textarea,p,blockquote,th,td,figure,button,time,span,i{margin:0; padding:0;-webkit-tap-highlight-color:rgba(0,0,0,0);-webkit-text-size-adjust:none;}
table{border-collapse:collapse; border-spacing:0;}
fieldset,img,button{border:0}
address,b,caption,cite,code,dfn,em,th,var{font-style:normal; font-weight:normal; font-variant:normal}
ol,ul{list-style:none}
caption,th{text-align:left}
h1,h2,h3,h4,h5,h6{font-size:100%; font-weight:normal}
q:before,q:after{content:""}
blockquote, q{quotes: none}
abbr,acronym{border:0}
a{text-decoration: none; color:inherit}
i,b,em,strong{font-style:normal}
input,button,select,textarea{outline:none}

.fb{font-weight:bold;}

/*position*/
.bcenter{margin:0 auto}
.tcenter{text-align:center}
.fl{float:left}
.fr{float:right}
.pr{position:relative}
.pa{position:absolute}
.pf{position:fixed}
.oh{overflow:hidden}
.none{display:none; visibility:hidden; width:0; height:0; overflow:hidden;}
.block{display:block}


/* CSS Document */
body{ background:#FFFEEB;}
a{ cursor:pointer;}
#toast{bottom:-10em; width:100%; font-size:1.5em; opacity:0; background:#fff; height:200px; line-height:200px; z-index:100;}
/*animation & transition*/
.toastAnimation{-webkit-animation:toast 3s linear 1;}
@-webkit-keyframes toast{
	0%{opacity:0;}
	30%{opacity:1;}
	70%{opacity:1;}
	100%{opacity:0;}
}
.btn{ font-size:12px; padding:6px 20px; background:#D8252F; border-bottom:1px solid #94181F; color:#fff;}
.btn_grey{ font-size:12px; padding:6px 20px; background:#999; border-bottom:1px solid #666; color:#fff;}
.wrap{ max-width:360px; margin:0 auto;}
.main_bg{ background:url(../images/banner.png) no-repeat center top #FFD782; background-size:100% auto;}
.banner{ width:100%; overflow:hidden; position:relative;}
.banner_top{ padding-top:10px; overflow:hidden;}
.banner_top img{ width:64px;}
.banner_bottom{ width:100%; position:absolute; bottom:0px; text-align:center; left:40px;}
.banner_bottom img{ width:91px;}
.con1{ width:310px; margin:0 auto;}
.wish_box{ width:auto; height:35px; overflow:hidden; text-align:center; background:#EBB545; padding:10px 0px 4px; border-radius:8px;}
.wish_box label{ font-size:12px; color:#562713;}
.wish_box input{ background:#fff; width:140px; height:24px; line-height:24px; font-size:12px; color:#562713; border:2px solid #B05B00; border-radius:4px; padding:0px 5px;}
.wish_list{ width:100%;}
.wish_list h2{ font-size:12px; color:#bb5529; line-height:30px;}
.wish_txt{ width:100%; overflow:hidden;}
.wish_txt span{ font-size:12px; color:#562713; background:#fff; padding:5px 8px; float:left; margin-right:5px; margin-bottom:10px; box-shadow:0 5px 10px 0 #C09948;}
.con2{ width:100%; margin:0 auto; overflow:hidden;}
.con2 h2 img{ width:100%; vertical-align:top;}
.pool_wrap{ width:100%; height:125px;  background:#7499C1; position:relative;}
.water{ width:620px; height:140px; background:url(../images/water.png) no-repeat 0 0; position:absolute; left:0px; bottom:0px; z-index:1; opacity:0.8;-webkit-animation:water 10.0s linear infinite;}
.pool{ width:310px; height:125px; margin:0 auto; position:relative; z-index:10;}
.pool_txt{ width:100%; text-align:center; margin:20px 0px;}
.pool_txt span{ font-size:12px; color:#562713; position:relative; padding:5px 20px 5px 40px; background:#EFBE56; border-radius:10px; margin-right:10px;}
.pool_txt span img{ position:absolute; bottom:0px; left:10px; width:21px;}
.pool_txt span strong{ color:#d8242d; margin:0px 2px;}
.pool_txt a{ font-size:12px; color:#d8242d; text-decoration:underline;}
.shareToWeibo{ width:310px; margin:0 auto; padding:10px 0px; border-top:1px dashed #D0A663;}
.shareToWeibo dl{ overflow:hidden;}
.shareToWeibo dl dt{ float:left;}
.shareToWeibo dl dd{ float:right;}
.shareToWeibo dl dt h3{ font-size:12px; color:#562713;}
.shareToWeibo dl dt h4{ font-size:11px; color:#bb5529;}
.con3{ position:relative; background:#EFBE55; margin:10px 0px;}
.ledou_intro{ width:320px; margin:0 auto; background:url(../images/ledou_bg.png) no-repeat center; height:295px; background-size:contain; position:relative;}
.ledou_intro .btn{ position:absolute;}
.ledou_intro .ledou_btn1{ top:130px; left:110px;}
.ledou_intro .ledou_btn2{ top:175px; left:210px;}
.ledou_intro .ledou_btn3{ top:250px; left:110px;}
.ledou_intro h2{ position:absolute; top:240px; left:220px; font-size:12px; color:#562713; line-height:20px;}
.ledou_intro h2 strong{ color:#d8242d; margin:0px 2px;}
.ledou_intro h2 a{ color:#d8242d; text-decoration:underline;}
.con4{ width:310px; margin:0 auto;}
.con4_title{ text-align:center; width:100%; margin-bottom:10px;}
.con4_title h2 img{ width:250px;}
.con4_title h3{ font-size:12px; color:#562713; line-height:24px;}
.con4_title h4{ font-size:11px; color:#bb5529;}
.app_list{ width:100%; overflow:hidden;}
.app_list li{ border-top:1px dashed #D0A663; padding:10px 0px; height:70px;}
.app_list li .appImg{ width:60px; height:60px; position:relative; margin:0px 10px;}
.app_list li .appImg img{ width:60px; height:60px; position:absolute; top:0px; left:0px; z-index:2;}
.app_list li .appImg i{ width:77px; height:12px; background:url(../images/icon2.png) no-repeat center; background-size:contain; position:absolute; bottom:-6px; left:-8px; z-index:1;}
.app_list li .fr{ text-align:right;}
.app_list li .appInfo h2{ font-size:18px; color:#562713; font-weight:bold; line-height:30px;}
.app_list li .appInfo h3{ font-size:12px; color:#bb5529; margin-left:2px;}
.app_list li .appStatus{ margin-top:15px;}
.bottom_ad{ width:100%; text-align:center; margin-bottom:10px;}
.bottom_ad img{ width:100%;}
.con5{ text-align:center; padding:10px 0px; background:#EFBE56;}
.con5 span{ font-size:14px; color:#562713;}
.con5 span img{ margin-right:10px; vertical-align:middle; width:20px;}

.bottle_item{ width:82px; height:70px; position:absolute; overflow:hidden;}
.wave{ width:82px; height:6px; background:url(../images/wave.png) no-repeat center bottom #3FB6FD; background-size:auto 6px; position:absolute; left:0px; bottom:0px; z-index:2;}
.bottle{ width:30px; height:60px; background:url(../images/bottle.png) no-repeat 0 0; position:absolute; background-size:contain; left:50%; margin-left:-15px; cursor:pointer;}
.item1{ top:-25px; left:18px; z-index:5;-webkit-animation:wave1 2s linear infinite;}
.item2{ top:-35px; left:180px; z-index:4;-webkit-animation:wave2 2s linear infinite;}
.item3{ top:5px; left:105px; z-index:3;-webkit-animation:wave3 2s linear infinite;}
.item4{ top:40px; left:50px; z-index:2;-webkit-animation:wave4 3s linear infinite;}
.item5{ top:20px; left:175px; z-index:1;-webkit-animation:wave5 2.5s linear infinite;}
.bottle1{ bottom:-20px;-webkit-animation:bottle1 5.0s linear infinite;}
.bottle2{ bottom:-5px;-webkit-animation:bottle2 5.0s linear infinite;}
.bottle3{ bottom:-10px;-webkit-animation:bottle3 8.0s linear infinite;}
.bottle4{ bottom:-25px;-webkit-animation:bottle4 5.0s linear infinite;}
.bottle5{ bottom:-15px;-webkit-animation:bottle5 5.0s linear infinite;}
.animal1{ width:27px; height:23px; background:url(../images/animal1.png) no-repeat 0 0; background-size:contain; position:absolute; top:5px; left:0px;-webkit-animation:animal1 15.0s linear infinite;}
.animal2{ width:38px; height:13px; background:url(../images/animal2.png) no-repeat 0 0; background-size:contain; position:absolute; top:50px; left:0px;-webkit-animation:animal2 8.0s linear infinite;}

.header{ width:100%; position:relative; height:43px; line-height:43px; background:#D8242D;}
.header h1{ font-size:18px; text-align:center; color:#fff; width:100%; height:100%; position:absolute; top:0px; left:0px; font-weight:bold;}
.back_btn{ float:left; margin-left:10px; font-size:14px; color:#fff; padding-left:12px; background:url(../images/arrow2.png) no-repeat left center; position:relative; z-index:2; background-size:8px auto;}
.content{ padding:15px;}
.content h2{ font-size:14px; line-height:20px; margin-top:10px; font-weight:bold; color:#fa5327;}
.content h3{ font-size:12px; line-height:20px; font-weight:bold; color:#191919;}
.content p{ font-size:10px; line-height:20px; color:#191919;}
.content p strong{ color:#fa5327;}
.line{ width:100%; height:0px; border-bottom:1px dashed #D0C2AF; margin-bottom:20px;}

.allprize_none{ text-align:center; padding-bottom:25px;}
.allprize_none h2{ font-size:16px; color:#464646; line-height:20px; margin-bottom:20px;}
.allprize_none a{ margin:0px 5px; font-size:15px;}
.allprize_none .img_wrap img{ width:97px;}

.contact{ width:310px; margin:0 auto; padding-bottom:40px; clear:both; border-top:1px solid #E7E6DD; padding-top:20px;}
.contact h2{ font-size:14px; color:#363636; font-weight:bold; padding-left:15px; margin-bottom:8px;}
.contact h3{ font-size:10px; color:#191919; padding-left:15px; line-height:20px;}
.contact h4{ font-size:13px; color:#424242; text-align:center;}
.contact .img_wrap{ text-align:center; margin:10px 0px;}
.contact .img_wrap img{ width:80px;}

.tab_prize{ width:100%; padding-bottom:25px;}
.tab_prize_con{ position:relative; overflow:hidden;}
.prizeList_title{ width:100%; font-size:18px; color:#fff; background:#204691; height:44px; line-height:44px;}
.prizeList_title h2{ margin-left:15px; font-weight:bold;}
.prize_title{ padding:15px; line-height:20px; overflow:hidden;}
.prize_title h2{ font-size:12px; color:#464646; line-height:20px; background:#FFE8B9; padding:5px; text-align:center; border-radius:8px; margin-bottom:10px;}
.prize_title h2 a{ color:#d71e2c; text-decoration:underline;}
.prize_title h3{ font-size:12px; color:#464646; overflow:hidden;}
.prize_title h3 em{ font-weight:bold; color:#d71e2c;}
.prize_title h3 span a{ color:#d71e2c; text-decoration:underline;}
.prize_title h4{ font-size:10px; color:#757575; clear:both;}
.prize_list{ padding:0px 15px;}
.prize_list li{ padding:15px 0px; border-bottom:1px solid #D3D3D3; overflow:hidden; clear:both; text-align:left; line-height:20px;}
.prize_list_l{ width:70%;}
.prize_list_l h2{ font-size:14px; color:#191919; font-weight:bold;}
.prize_list_l h3{ font-size:10px; color:#191919; font-weight:bold;}
.prize_list_l h4{ font-size:10px; color:#757575;}
.prize_list_l h4 span{ width:100px; display:inline;}
.prize_tips{ clear:both; text-align:center; height:40px;}
.prize_tips span{ font-size:12px; color:#464646; background:#FFD98A; border-radius:8px; padding:15px 10px; margin:0 auto;}
.prize_tips span strong{ color:#d8242d;}
.prize_tips span a{ margin-left:10px;}
.prize_info{ padding:30px 30px 0px; text-align:left;}
.prize_info h2{ clear:both; overflow:hidden; font-size:14px; color:#fa5327; font-weight:bold; line-height:30px;}
.prize_info h3{ font-size:12px; color:#757575;}
.prize_info h4{ font-size:10px; color:#191919;}
.prize_none{ padding:25px 0px; text-align:center;}
.prize_none h2{ font-weight:bold; font-size:12px; color:#757575; margin-bottom:10px;}

#mask{ width:100%; height:100%; z-index:100; top:0px; left:0px; background:#000; opacity:0.5;}
#dialogWrapper{ width:90%; z-index:200; left:0px; top:0px; background:#fff; overflow:hidden;}
.pop_wrap{ padding:10px; margin:0 auto; width:90%;}
.pop_wrap h2{ font-size:18px; color:#191919; margin-bottom:10px; text-align:center;}
.pop_wrap h2 strong,.pop_wrap h3 strong{ color:#fa5327;}
.pop_wrap h3{ font-size:12px; color:#000000; width:85%; margin:0 auto; line-height:20px;}
.pop_wrap h4{ font-size:12px; color:#757575; margin-top:10px; width:85%; margin:0 auto;}
.pop_wrap h5{ font-size:15px; color:#191919; width:85%; margin:0 auto;}
.pop_wrap p{ font-size:12px; color:#757575; width:85%; margin:0 auto; line-height:15px; margin-bottom:10px;}
.pop_wrap p strong{ color:#fa5327;}
.btn_box{ text-align:center; margin-top:10px;}
.btn_box a,.btn_box span{ width:90px; margin:0px 10px;}
.winner_info{ border-top:1px solid #D5D5D5; width:85%; margin:0 auto; padding-top:5px;}
.winner_info p{ width:100%; margin-bottom:10px; clear:both; overflow:hidden;}
.winner_info p label{ width:32%; float:left;}
.winner_info p input{ width:60%; float:left; height:15px; border:1px solid #B2B2B2; background:#fff; padding:0px 2px;}
.winner_info p select{ margin-right:2px; height:15px; border:1px solid #B2B2B2; background:#fff;}
.winner_info p.select_type1 select{ width:32%;}
.winner_info p.select_type2 select{ width:65%;}
.winner_info p.address input{ width:90%;}

.winner_list{ width:100%;}
.winner_list h2{ font-size:14px; color:#f76b00; font-weight:bold; padding-left:15px;}
.winner_list h2 img{ width:100%;}
.winner_list h3{ font-size:10px; color:#757575; font-weight:bold; padding-left:15px; margin-bottom:15px;}
.winner_list h3 a{ color:#d71e2c;}
.winner_list_con{ margin:15px; background:#FFFDD2; overflow:hidden;}
.scrollList_wrap{ margin:15px; height:150px; line-height:15px; position:relative; overflow:hidden; font-size:10px;}
#scrollList{ position:absolute; top:0px; left:0px; white-space:nowrap; height:auto; line-height:15px;}
.scrollList_wrap{ display:block;}
.scrollList_wrap span{ color:#555555; width:100%; display:block;}

@-webkit-keyframes water{
	0% { left:0px;}
  50% { left:-260px;}
  100% { left:0px;}
}
@-webkit-keyframes wave1{
	0% { left:18px;}
   25% { left:22px;}
  50% { left:24px;}
  75% { left:22px;}
  100% { left:18px;}
}
@-webkit-keyframes wave2{
	0% { left:180px;}
   25% { left:176px;}
  50% { left:174px;}
  75% { left:176px;}
  100% { left:180px;}
}
@-webkit-keyframes wave3{
	0% { left:105px;}
   25% { left:108px;}
  50% { left:110px;}
  75% { left:108px;}
  100% { left:105px;}
}
@-webkit-keyframes wave4{
	0% { left:50px;}
   25% { left:54px;}
  50% { left:58px;}
  75% { left:54px;}
  100% { left:50px;}
}
@-webkit-keyframes wave5{
	0% { left:175px;}
   25% { left:172px;}
  50% { left:170px;}
  75% { left:172px;}
  100% { left:175px;}
}
@-webkit-keyframes bottle1{
	0% { bottom:-20px; -webkit-transform:rotate(0deg);}
   25% { bottom:-25px; -webkit-transform:rotate(-10deg);}
  50% { bottom:-20px; -webkit-transform:rotate(0deg);}
  75% { bottom:-15px; -webkit-transform:rotate(10deg);}
  100% { bottom:-20px; -webkit-transform:rotate(0deg);}
}
@-webkit-keyframes bottle2{
	0% { bottom:-5px; -webkit-transform:rotate(-15deg);}
   25% { bottom:-20px; -webkit-transform:rotate(-8deg);}
  50% { bottom:-15px; -webkit-transform:rotate(0deg);}
  75% { bottom:-10px; -webkit-transform:rotate(-8deg);}
  100% { bottom:-5px; -webkit-transform:rotate(-15deg);}
}
@-webkit-keyframes bottle3{
	0% { bottom:-10px; -webkit-transform:rotate(15deg);}
   25% { bottom:-20px; -webkit-transform:rotate(0deg);}
  50% { bottom:-10px; -webkit-transform:rotate(-15deg);}
  75% { bottom:-8px; -webkit-transform:rotate(0deg);}
  100% { bottom:-10px; -webkit-transform:rotate(15deg);}
}
@-webkit-keyframes bottle4{
	0% { bottom:-25px; -webkit-transform:rotate(10deg);}
   25% { bottom:-30px; -webkit-transform:rotate(0deg);}
  50% { bottom:-25px; -webkit-transform:rotate(-10deg);}
  75% { bottom:-28px; -webkit-transform:rotate(0deg);}
  100% { bottom:-25px; -webkit-transform:rotate(10deg);}
}
@-webkit-keyframes bottle5{
	0% { bottom:-15px; -webkit-transform:rotate(-10deg);}
   25% { bottom:-20px; -webkit-transform:rotate(-15deg);}
  50% { bottom:-25px; -webkit-transform:rotate(-10deg);}
  75% { bottom:-20px; -webkit-transform:rotate(0deg);}
  100% { bottom:-15px; -webkit-transform:rotate(-10deg);}
}
@-webkit-keyframes animal1{
	0% { left:360px;}
   25% { left:260px;}
  50% { left:160px;}
  75% { left:80px;}
  100% { left:-50px;}
}
@-webkit-keyframes animal2{
	0% { left:360px;}
   25% { left:240px;}
  50% { left:130px;}
  75% { left:50px;}
  100% { left:-80px;}
}