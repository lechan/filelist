<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>无标题文档</title>
</head>

<body>
<iframe src="b.html" id="abc_ifm"></iframe>
<script type="text/javascript">
document.domain = 'lechan.duapp.com';
function iframeLoaded(iframeEl,callback) {
	if(iframeEl.attachEvent) {
		iframeEl.attachEvent("onload", function() {
			if(callback && typeof(callback) == "function") {
				callback();
			}
		});
	} else {
		iframeEl.onload = function() {
			if(callback && typeof(callback) == "function") {
				callback();
			}
		}
	}
}

function callback(){
	alert(document.getElementById('abc_ifm').contentWindow.document.getElementById('abc').innerHTML);	
}

iframeLoaded(document.getElementById('abc_ifm'),callback)

</script>
</body>
</html>
