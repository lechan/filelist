<script>
    var top0 = $(".conber").eq(0).offset().top - 64;
    var top1 = $(".conber").eq(1).offset().top - 64;
    var top2 = $(".conber").eq(2).offset().top - 64;
    var top3 = $(".conber").eq(3).offset().top - 64;
    var top4 = $(".conber").eq(4).offset().top - 64;
    var top5 = $(".conber").eq(5).offset().top - 64;
    var top6 = $(".conber").eq(6).offset().top - 64;
	var top7 = $(".conber").eq(7).offset().top - 64;
	
    $("#retop div").eq(0).click(function () {
        qtop(top0);
    });
    $("#retop div").eq(1).click(function () {
        qtop(top1);
    });
    $("#retop div").eq(2).click(function () {
        qtop(top2);
    });
    $("#retop div").eq(3).click(function () {
        qtop(top3);
    });
    $("#retop div").eq(4).click(function () {
        qtop(top4);
    });
    $("#retop div").eq(5).click(function () {
        qtop(top5);
    });
    $("#retop div").eq(6).click(function () {
        qtop(top6);
    });
	$("#retop div").eq(7).click(function () {
        qtop(top7);
    });
	
    $("#retop .m_1").click(function () {
        qtop(0);
    });
    function qtop(dtop) {
        $('body,html').animate({scrollTop: dtop}, 100);
    }
    ;
    $(window).scroll(function () {
        var t = $(document).scrollTop();
        var con;
        var con2;
        $(".conber").each(function (i) {
            var thist = $(this).offset().top;
            if (t > thist - $(this).parent().offset().top) {
                con = thist;
                con2 = i;
            }
        });
        $("#retop div").removeClass("active");
        $("#retop div").eq(con2).addClass("active");
        if ($(document).scrollTop() > 0) {
            $("#retop").fadeIn();
        } else if ($(document).scrollTop() <= 0) {
            $('#retop').stop().fadeOut();
        }
        ;
    });
</script>
