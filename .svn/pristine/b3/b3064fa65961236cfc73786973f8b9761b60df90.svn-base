<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>无标题文档</title>
<meta content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1.0,user-scalable=no" name="viewport" id="viewport" />
<link rel="stylesheet" type="text/css" href="css/6.css" />
</head>

<body>
<div id="wrap"></div>
<script type="text/javascript">
var html = '';
for(var i=0;i<6;i++){
	html += '<div class="group">'
	for(var j=0;j<18;j++){
		html += '<span></span>';	
	}
	html += '</div>'
}
document.getElementById("wrap").innerHTML = html;
</script>
</body>
</html>
