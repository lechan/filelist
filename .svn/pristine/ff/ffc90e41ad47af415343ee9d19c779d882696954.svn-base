<!--设置页-->
<section class="page_item" id="page_set">
<header class="page_header">
	<div class="fl header_left"><span class="back_btn"></span></div>
    <h1 class="header_title">设置</h1>
</header>
<ul class="set_list">
	<li data-name="setBg"><label>左侧菜单背景</label><span>默认</span></li>
    <li data-name="setFontsize"><label>字体大小</label><span>中</span></li>
    <li data-name="clearBuffer"><label>清除缓存</label></li>
</ul>
</section>

<!--设置弹窗-->
<section class="pop" id="setBg">
	<dl>
    	<dt>选择左边菜单背景</dt>
        <dd data-info="type1">默认</dd>
        <dd data-info="type2">亮蓝色</dd>
        <dd data-info="type3">深蓝色</dd>
    </dl>
    <div class="close_pop">取消</div>
</section>

<section class="pop" id="setFontsize">
	<dl>
    	<dt>选择新闻内容字体</dt>
        <dd data-info="type1">大</dd>
        <dd data-info="type2">中</dd>
        <dd data-info="type3">小</dd>
    </dl>
    <div class="close_pop">取消</div>
</section>

<section class="pop" id="clearBuffer">
	<dl>
    	<dt>清除缓存</dt>
        <dd>确认</dd>
    </dl>
    <div class="close_pop">取消</div>
</section>


