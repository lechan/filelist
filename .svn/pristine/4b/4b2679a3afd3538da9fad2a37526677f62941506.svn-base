@charset "utf-8";body,div,dl,dt,dd,h1,h2,h3,h4,form,input,textarea,p,button,span,i,ul,li,del,em{margin:0; padding:0; -webkit-tap-highlight-color:rgba(0,0,0,0);}body{font-family: "microsoft yahei"; font-size:22px; margin: 0; color: #b2b2b2; line-height:1; background-color: #f2f2f2; min-width:320px;}ul{list-style: none;}i,em{font-style: normal;}a,a:active{text-decoration: none; color: #b2b2b2;}h2{font-size: 16px; font-weight: normal;}h3{font-size: 14px; font-weight: normal;}h4{font-size: 12px; font-weight: normal;}img{border: none; vertical-align:top;}.clearfix:after {visibility: hidden; display: block;font-size: 0; content: " "; clear: both;height: 0;}.clearfix{*zoom:1;}.pa{position:absolute}.pf{position:fixed}.tcenter{text-align:center}.fl{float:left;}.fr{float:right;}.wrap{max-width:640px; min-width:320px; margin:0 auto; overflow:hidden; background:-webkit-linear-gradient(top, red, rgba(0, 0, 255, 0.5)); -webkit-transition:all ease-out 0.3s; position:relative; z-index:3;}.content{width:100%;}.sidebar{width:50%; overflow:hidden; position:fixed; z-index:1; top:0px;}.sidebar li{width:100%; height:30px; line-height:30px; font-size:12px; color:#666; border-bottom:1px solid #666;}.swipeL{margin-right:50%;}.swipeR{margin-left:50%;}.banner{width:100%; position:relative; overflow:hidden;}.banner_wrap{width:100%; overflow:hidden; position:relative;}.banner_wrap div{float:left; width:100%; position: relative; overflow:hidden;}.banner_wrap div img{width:100%;}.banner_dot_wrap{width:100%; overflow:hidden; position:absolute; bottom:10px; left:0px; text-align:center; height:5px;}.banner_dot_wrap i{width:20px; height:5px; background:#000; opacity:0.4; display:inline-block; margin:0px 2px; vertical-align:top; -webkit-transition:all ease-out 0.2s;}.banner_dot_wrap i.active{opacity:0.8;}#recommend_wrapper {position: relative; max-width:640px; width: 100%; margin:0 auto; background: #ccc; overflow: hidden; height:800px;}#scroller {position: absolute; z-index: 1; -webkit-tap-highlight-color: rgba(0,0,0,0); width: 2020px; height: 100%; background-color: #a00; -webkit-transform: translateZ(0); -moz-transform: translateZ(0); -ms-transform: translateZ(0); -o-transform: translateZ(0); transform: translateZ(0); -webkit-touch-callout: none; -webkit-user-select: none; -moz-user-select: none; -ms-user-select: none; user-select: none; -webkit-text-size-adjust: none; -moz-text-size-adjust: none; -ms-text-size-adjust: none; -o-text-size-adjust: none; text-size-adjust: none;}#scroller ul {list-style: none; padding: 0; margin: 0; width: 100%; height: 100%; text-align: center;}#scroller li {display: block; float: left; width: 100px; height: 100%; border-right: 1px solid #ccc; background-color: #fafafa; font-size: 14px;}.listItem{width:100%; overflow:hidden; background:#fff;}.listItem h2{border-top:1px solid #D1D1D1; border-bottom:1px solid #D1D1D1; padding:10px; overflow:hidden;}.listItem span{font-size:20px;}.listItem a{font-family:"宋体"; font-size:20px;}.listWrap{overflow:hidden;}.listWrap li{width:33%; float:left; border-right:1px solid #D1D1D1;}.listWrap li:nth-child(3n){border:none;}.listImg{text-align:center; padding:20px 0px 10px;}.listImg img{width:90%;}.listWrap li a{width:100%; display:block; overflow:hidden;}.listWrap li h3{padding:0px 10px; font-size:14px; line-height:20px;}.listWrap li h4{padding:0px 10px; font-size:12px; line-height:20px; margin-bottom:10px;}.listWrap li h4 strong{font-size:16px; font-weight:100;}#mask{width:100%; height:100%; z-index:100; top:0px; left:0px; background:rgba(0,0,0,0.5); display:none;}#dialogWrapper{width:90%; z-index:200; left:0px; top:0px; overflow:hidden;}.pop_wrap{padding:10px; background:#fff;}#toast{bottom:-10em; font-size:12px; opacity:0; background:#000; color:#fff; padding:5px 10px; z-index:100; border-radius:5px; left:50%;}.toastAnimation{-webkit-animation:toast 3s linear 1;}@-webkit-keyframes toast{0%{opacity:0;} 30%{opacity:0.7;} 70%{opacity:0.7;} 100%{opacity:0;}}