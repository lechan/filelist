<script type="text/javascript" src='js/zepto.js'></script>
<script type="text/javascript" src='js/video.js'></script>
<script type="text/javascript" src='js/detail.js'></script>
<script type="text/javascript">
//page tracking
var EVENT_ACTION='detail';
window.Avatar= [];
Avatar.push([document.querySelectorAll("#navList a"), 'clickDetailTab']);
Avatar.push([document.querySelector("#btnError"), 'clickWrongReport']);
Avatar.push([document.querySelector("#btnSort"), 'clickVideoOrder']);
Avatar.push([document.querySelector("#btnShare"), 'clickVideoShare']);
Avatar.push([document.querySelector("#detailSourceBox"), 'changeVideoSource']);
Avatar.push([document.querySelector("#btnOL"), 'clickOffline']);
Avatar.push([document.querySelector("#btnPlay"), 'clickPlayVideo']);
Avatar.push([document.querySelectorAll("#videoList"), {selector:".drama-item", parser:'clickPlayVideo'}]);
Avatar.push([document.querySelector("#btnCmt"), 'clickCommentBtn']);
Avatar.push([document.querySelectorAll("#reviewList"),  {selector:".icon-comment", parser:'clickCommentListBtn'}]);
(function(){
	var scriptTag = document.createElement('script');
	scriptTag.type = 'text/javascript';
	scriptTag.async = true;
	scriptTag.src = 'js/avatarCfg.js';
	var t=document.getElementsByTagName("script")[0];t.parentNode.insertBefore(scriptTag,t);
})()
</script>
</body>
</html>

