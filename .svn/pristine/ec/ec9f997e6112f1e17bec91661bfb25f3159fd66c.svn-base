@charset "utf-8";

/* CSS reset */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fiedlset,input,textarea,p,blockquote,th,td,figure,button,time,span,i{margin:0; padding:0;-webkit-tap-highlight-color:rgba(0,0,0,0);}
table{border-collapse:collapse; border-spacing:0;}
fieldset,img,button{border:0}
address,b,caption,cite,code,dfn,em,th,var{font-style:normal; font-weight:normal; font-variant:normal}
ol,ul{list-style:none}
caption,th{text-align:left}
h1,h2,h3,h4,h5,h6{font-size:100%; font-weight:normal}
q:before,q:after{content:""}
blockquote, q{quotes: none}
abbr,acronym{border:0}
a{text-decoration: none; color:inherit}
i,b,em,strong{font-style:normal}
input,button,select,textarea{outline:none}

.fb{font-weight:bold;}

/*position*/
.bcenter{margin:0 auto}
.tcenter{text-align:center}
.fl{float:left}
.fr{float:right}
.pr{position:relative}
.pa{position:absolute}
.pf{position:fixed}
.oh{overflow:hidden}
.none{display:none; visibility:hidden; width:0; height:0; overflow:hidden;}
.block{display:block}


/* CSS Document */
html{ height:100%;}
body{ line-height:1; height:100%; background:#3A3840;}
#masking{width:100%; top:0; left:0; background-color:#000; opacity: .6; filter: alpha(opacity=60) \9; z-index:1000; display:none; height:100%;}
#wxmask{width:100%; top:0; left:0; background-color:#000; opacity: .6; filter: alpha(opacity=60) \9; z-index:2000; display:none; height:100%;}
#toast{z-index:999; padding:10px; color:#fff; font-size:16px; background-color:#000; opacity:.8; +filter: alpha(opacity=80); border-radius:6px; box-shadow: 0px 1px 5px 0px #000; display:none; bottom:75px}
.icon{background:url(../images/icons.png) no-repeat; display:block}
a{ cursor:pointer;}
/*animation & transition*/
.toastAnimation{-webkit-animation:toast 3s linear 1;}
@-webkit-keyframes toast{
	0%{opacity:0;}
	30%{opacity:1;}
	70%{opacity:1;}
	100%{opacity:0;}
}
.wrap{ max-width:640px; height:100%; margin:0 auto; position:relative;}
.start_box{ width:100%; height:100%; background:url(../images/bg.png) no-repeat center top; background-size:contain; position:absolute; top:0px; left:0px; z-index:3; text-align:center; display:none;}
.start_box img{ margin-top:80%;}
#start_btn{ display:none;}
.content_box{ width:100%; height:100%; position:absolute; top:0px; left:0px; z-index:2; background:url(../images/bg2.png) no-repeat center top; background-size:contain; display:none;}
.content_box h2{ font-size:40px; color:#fff; text-align:center; margin:5% 0px;}
.app_list{ width:100%; position:relative;}
.list_item{ width:100%; position:absolute; top:0px; left:0px; text-align:center; display:none;}
.list_item dl{ margin-bottom:25%;}
.list_item dl dt{ text-align:center; margin-bottom:5%;}
.list_item dl dt img{ width:200px;}
.list_item dl dd{ font-size:38px; color:#ffecd2;}
.list_item ul{ text-align:center;}
.list_item ul li{ width:550px; height:90px; line-height:90px; font-size:40px; font-weight:bold; color:#3a3840; background:url(../images/item_bg.png) no-repeat center; margin:0 auto 5%; text-align:center; background-size:contain; cursor:pointer;}

/*ox弹窗*/
#right_answer,#wrong_answer,#time_up{ width:550px; height:390px; background:url(../images/bg_pop.png) center; background-size:contain; text-align:center; overflow:hidden; display:none;}
#right_answer img,#wrong_answer img,#time_up img{ margin-top:10%;}
#right_answer h3,#wrong_answer h3,#time_up h3{ font-size:32px; font-weight:bold;}

/*结果弹窗*/
#result{ width:550px; height:852px; background:url(../images/bg_pop2.png) no-repeat center; background-size:contain; display:none; text-align:center;}
#result img{ margin:10% 0px 5%;}
#result span{ color:#ec7063;}
#result div{ visibility:hidden;}
#result h2{ font-size:42px; color:#000000; font-weight:bold; margin-bottom:5%; height:auto;}
#result h3{ font-size:28px; color:#464646; line-height:40px; height:auto;}
#result a{ width:405px; height:70px; line-height:70px; font-size:40px; display:block; font-weight:bold; color:#3a3840; background:url(../images/item_bg2.png) no-repeat center; text-align:center; background-size:contain; cursor:pointer; margin:5% auto;}
#result .loading{ width:100%; height:100%; position:absolute; top:0px; left:0px; z-index:-1; background:url(../images/loading.gif) no-repeat center;}
#main_loading{ width:100%; height:100%; position:absolute; top:0px; left:0px; z-index:3; background:url(../images/loading.gif) no-repeat center #fff;}

/*分享 提示对画框*/
.dialog{width:300px; top:0px; left:0px; z-index:1500; color:#191919; background-color:#fff;-webkit-transform-style:preserve-3d;}
.dialog p,.dialog h3{height:60px; line-height:60px;}
.dialog .title{font-size:22px}
.dialog .cancel, .dialog .confirm{font-size:18px; cursor:pointer}
.dialog .confirm{color:#fe8800}
#share{display:none;border:2px solid #000;border-radius:5px; box-shadow:0 2px 3px 0 #000;}
#share ul{border-bottom:1px solid #a3a3a3; border-top:1px solid #a3a3a3; font-size:16px; width:100%; position:relative; z-index:2; overflow:hidden;}
#share li{ width:33.33%; height:100px; float:left; background:#fff; text-align:center; font-size:14px;}
#share li a{cursor:pointer; width:100%; display:block;}
#share li i{width:54px; height:54px; margin:15px auto 5px;}
#share li i.qzone{background-position:0px -100px}
#share li i.sina{background-position:-54px -100px}
#share li i.txwb{background-position:-108px -100px}
#share li i.rr{background-position:-162px -100px}
#share li i.db{background-position:-216px -100px}
#share li i.qq{background-position:-270px -100px}
#share li i.wx{background-position:-324px -100px}
#share li i.pyq{background-position:-378px -100px}
.wxshare{ display:none;}


/*提示对话框*/
#tipDialog, #updateDialog{display:none;border:2px solid #000;border-radius:5px; box-shadow:0 2px 3px 0 #000;}
#tipDialog .cancel, #tipDialog .confirm, #updateDialog .cancel, #updateDialog .confirm{float:left; width:49.5%; text-align:center}
#tipDialog .content, #updateDialog .content{font-size:14px; border-bottom:1px solid #a3a3a3; border-top:1px solid #a3a3a3; height:80px; line-height:80px}
#tipDialog .splitLine, #updateDialog .splitLine{width:1px; background-color:#ccc; height:30px; left:50%; top: 14px}
#mmTip{background-image:url(../images/mm.png); background-size:235px auto; width:235px; height:180px; top:0; right:0; z-index:3000; display:none}
.animate_popOn{-webkit-animation:dialogPopOn 0.2s ease-in-out;}
.animate_popOff{-webkit-animation:dialogPopOff 0.2s ease-in-out;}
.animate_maskOn{-webkit-animation:maskOn 0.2s ease-in-out;}
.animate_maskOff{-webkit-animation:maskOff 0.2s ease-in-out;}

/* -------------------------------------
 * Bar container
 * ------------------------------------- */
.progress-radial {
	position:absolute;
	top:45%;
	left:50%;
	margin-left:-90px;
	width: 180px;
	height: 180px;
	border-radius: 50%;
	border: 10px solid #5d6771;
	background-color: #ffffff;
	box-shadow: 0 2px 15px rgba(0, 0, 0, 0.3);
}
.progress-radial:after, .progress-radial:before {
  content: '';
  width: 35px;
  height: 35px;
  top: 50%;
  left: 50%;
  border-radius: 50%;
  margin-left: -17.5px;
  margin-top: -17.5px;
  background: #ffffff;
  position: absolute;
  z-index: 999;
  box-shadow: 10px 0 10px rgba(0, 0, 0, 0.2);
}
.progress-radial:after {
  z-index: 998;
  box-shadow: none;
  -webkit-transform: translate(0, -72.5px);
}
.progress-radial b:after {
  color: #ffffff;
  text-shadow: 0 1px 2px rgba(0, 0, 0, 0.6);
  position: absolute;
  font-weight: 900;
  left: 50%;
  top: 50%;
  width: 50%;
  height: 50%;
  background-color: #2f3439;
  border-radius: 50%;
  margin-left: -25%;
  margin-top: -25%;
  text-align: center;
  line-height: 90px;
  font-size: 30px;
  box-shadow: 0 2px 3px rgba(0, 0, 0, 0.3) inset, 0 0 0 10px #5d6771;
  z-index:1000;
}


@media handheld and (max-width: 640px), screen and (max-device-width: 640px), screen and (max-width: 640px)  {

.start_box img{ width:200px;}
/*ox弹窗*/
#right_answer,#wrong_answer,#time_up{ width:275px; height:195px;}
#right_answer img{ width:102px;}
#wrong_answer img{ width:110px;}
#time_up img{ width:104px;}
#right_answer h3,#wrong_answer h3,#time_up h3{ font-size:16px;}
.content_box h2{ font-size:20px;}
.list_item dl dt img{ width:100px;}
.list_item dl dd{ font-size:19px;}
.list_item ul li{ width:275px; height:45px; line-height:45px; font-size:20px;}
#result{ width:275px; height:426px;}
#result img{ width:105px;}
#result h2{ font-size:21px;}
#result h3{ font-size:14px; line-height:20px;}
#result a{ width:200px; height:35px; line-height:35px; font-size:20px;}

.progress-radial {
	top:35%;
  -webkit-transform:scale(0.5,0.5);
}

}



@-webkit-keyframes dialogPopOn {
  	0% { -webkit-transform: scale(3,3); opacity: 0; }
  100% { -webkit-transform: scale(1,1); opacity: 1; }
}
@-webkit-keyframes dialogPopOff {
  	0% { -webkit-transform: scale(1,1); opacity: 1; }
  100% { -webkit-transform: scale(0.8,0.8); opacity: 0; }
}
@-webkit-keyframes maskOn{
	0% { opacity: 0; }
  100% { opacity: 0.6;}	
}
@-webkit-keyframes maskOff{
	0% { opacity: 0.6; }
  100% { opacity: 0;}	
}
@media only screen and (-Webkit-min-device-pixel-ratio: 1.5){
#share li i{ background-size:430px auto;}
#share li i.qzone{background-position:0px 0px;}
#share li i.sina{background-position:-54px 0px;}
#share li i.txwb{background-position:-108px 0px;}
#share li i.rr{background-position:-162px 0px;}
#share li i.db{background-position:-216px 0px;}
#share li i.qq{background-position:-270px 0px;}
#share li i.wx{background-position:-324px 0px;}
#share li i.pyq{background-position:-378px 0px;}	
}




