@charset "gb2312";
/* reset */
body,form,h1,h2,h3,h4,h5,ul,ol,li,dl,dt,dd,p,input,textarea,table,th,td,iframe{margin:0;padding:0}
body,input,textarea{font-size:12px;font-family:microsoft yahei}
input,textarea{outline:none;}
textarea{ resize:none;}
ul,ol{list-style:none}
img{border:0}
button,input {line-height:normal;*overflow:visible}
a{ text-decoration:none; cursor:pointer;}


.weibo_inp{ width:565px; margin:0 auto;}
.weibo_inp textarea{ width:550px; height:70px; border:1px solid #F5CD93; resize:none; overflow:auto; padding:5px;}
.func{ overflow:hidden; margin-bottom:10px;}
.func a{ width:50px; height:20px; padding-left:20px; float:left; display:inline; font-size:12px; color:#2b83b0; line-height:20px;}
.func a.face{ background:url(../images/face.png) no-repeat left center;}
.func a.img{ background:url(../images/img.png) no-repeat left center;}
.submit_btn{ float:right; padding:0px; width:auto; height:auto;}
.txt_list{ width:600px; clear:both; overflow:hidden; margin:0 auto;}
.txt_list ul li{ width:100%; overflow:hidden; clear:both; margin-bottom:10px;}
.txt_list ul li input{ float:left; vertical-align:middle; margin:20px 10px 0px 0px;}
.txt_list ul li div.txt_item{ width:563px; height:62px; background:url(../images/txt_bg.png) no-repeat 0 0; float:left; vertical-align:middle;}
.txt_list ul li div.txt_item p{ width:530px; margin:0 auto; line-height:20px; padding-top:10px;}

.faceItem li{float:left; width:30px;}
.lt{float:left;}
