#detail-top{
	background:url(../img/detail-bg.png) no-repeat 100% 100%;
	padding:48px 60px 25px 53px;
}
#detail-top-upper{
	margin-bottom:20px;
}
.detail-poster{
	width:174px;
	height:240px;
	margin-right:1.5em;
	background:url(../img/poster.png) no-repeat;
	background-size:100% 100%;
}
.detail-score{
	position:absolute;
	bottom:0;
	width:100%;
	height:30px;
	line-height:30px;
	background:rgba(0, 0, 0, 0.4);
	text-indent:0.5em;
	color:white;
}
.detail-info{
	padding-top:2px;
	color:#f9f9f9;
	font-size:28px;
	line-height: 1em;
}
.detail-info > li{
	margin-bottom:22px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.detail-info > li > label{
	color:#a2a2a2;
}
.detail-info > li.detail-source-line{
	overflow:visible;
	margin-bottom:0;
}
#detailSourceBox{
	background: rgba(192, 192, 192, 0.4);
	border-radius: 2px;
}
#detailSourceDd{
	font-size: 14px;
}
.detail-follow-box{
	text-align:center;
}
.detail-follow-btn{
	width:128px;
	background-color:#fe8800;
	color:white;
	height: 40px;
	line-height: 36px;
}
.detail-follow-btn:before{
	content:'+ ';
}
.detail-follow-btn:after{
	content:'追剧';
}

.detail-follow-btn.followed{
	border-color:rgb(247, 247, 247);
	background-color:rgb(247, 247, 247);
	color:#a2a2a2;
	cursor:default;
}
.detail-follow-btn.followed:before{
	content:'\2713 ';
}
.detail-follow-btn.followed:after{
	content:'已追剧';
}

.detail-lk-report{
	position:absolute;
	right:0;
	bottom:0;
	font-size:26px;
	color:#a2a2a2;
}
.detail-lk-share{
	width:44px;
	height:44px;
	background:url(../img/share.png) no-repeat;
	background-size:100% 100%;
}
.detail-lk-share:focus,
.detail-lk-share:active,
.detail-lk-share:hover{
	background-image:url(../img/share-on.png);
}


/****************************/
/**********************/
#proposal.empty{
	height:100%;
	background:url(../img/none-prop.png) no-repeat center 10% / 50% 50%;
}
#proposal.empty:after{
	content:'暂时没有推荐哦！' !important;
	line-height:2em;
	text-align:center;
	position:absolute;
	bottom:35%;
	width:100%;
	color:#727272;
}
/**********************/
.review-general{
	padding:30px 30px 20px;
}
.review-score-avg{ 
	border: 1px solid #d1d1d1; 
	margin-right: 50px; 
	font-size: 26px;
	text-align: center;
	width:36%;
}
.review-score-avg-tit{ 
	background-color: #d1d1d1;
	font-size: 22px;
	line-height: 2em;
}
.review-score-avg-body{ 
	padding:50px 0;
}
.review-score-avg-num{
	font-size:1.5em;
}
.review-score-avg-num:after{
	content:'分';
}

.review-score-detail{
	margin-top:4px;
	color:#727272;
	font-size:24px;
	line-height:1.5em;
}
.review-score-detail label{
	width:8em;
	display: inline-block;
}
.review-score-detail .score:after{
	content:'分';
}
.review-score-detail-top{
	color:#fe8800;
	font-size:30px;
	margin-bottom:45px;
	line-height:1em;
}
.review-score-detail-mid{
	height:4.5em;
	overflow:hidden;
}
.review-score-detail.full .review-score-detail-mid{
	height:auto;
}
.review-score-detail-btm-more{
	text-align:right;
	cursor:pointer;
}
.review-score-detail-btm-more:before{
	content:'';
	display:inline-block;
	top: -5px;
	position: relative;
	width: 0.7em;
	height: 0.7em;
	border-bottom:1px solid;
	border-right:1px solid;
	margin-right:0.5em;
	-webkit-transform:rotate(45deg);
	transform:rotate(45deg);
}
.review-score-detail.full .review-score-detail-btm-more:before{
	-webkit-transform:rotate(-135deg);
	transform:rotate(-135deg);
	vertical-align: bottom;
}

.review-btn{
	text-align:center;
	margin-bottom:32px;
}
.review-list{ border-top: 1px solid #e8e8e8; font-size: 22px;}
.review-list li{ border-bottom: 1px solid #e8e8e8; padding: 22px 0; position: relative;}
.recContent{ margin:0 116px 0 30px;}
.recContent .appStars{ margin-right: 10px;}
.recContent .model{ margin-right:12px;}
.recContent .ver{ margin-left: 12px;}
.review-list-desc{ line-height: 1.5; margin-top: 18px; color:#191919;}

.icon-comment{ width: 58px; height: 58px; position: absolute; top:22px; right:28px;
	background: url(../img/cmt.png) no-repeat;
	background-size: 100% 100%;
}
.icon-comment:active{background-image: url(../img/cmt-on.png);}
.recSub{
	border:1px solid #e8e8e8; border-bottom-color:#ccc ; background-color: #f8f7f7; margin: 18px 18px 0; position: relative;
	webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	-o-border-radius: 10px;
	border-radius: 10px;
}
.recSub li{ padding:22px 10px;}
.recSub li:last-child{ border-bottom: none;}
.recSubContent{ margin:0 98px 0 0px;}
.recSubContent .model{ margin-left: 12px;}
.recSub .icon-comment{ right: 8px;}
.arrowTop,.arrowTopIn{border-width: 0 12px 12px 12px; border-style: solid; border-color: transparent transparent #e8e8e8 transparent; top: -12px; width: 0; height: 0; position: absolute; top:-12px; left: 30px;}
.arrowTopIn{border-color: transparent transparent #f8f7f7; top: -10px;}
.iconLoading{background: url(../img/loading.gif) no-repeat; display: inline-block; background-size:100% 100% ; height: 40px; vertical-align: text-bottom; margin-right: 10px;}

/**********************/
#synopsisCtn{
	padding:40px 30px;
	font-size:26px;
	line-height:1.5em;
	color:#757575;
}
#synopsisCtn label{
	color:#1b1b1b;
	margin-top: 6px;
}
#synopsisCtn .keyword{
	display:inline-block;
	margin-bottom: 24px;
	padding: 10px 24px 16px 24px;
	line-height: 1em;
	margin-right: 0.5em;
}
#synopsisCtn a.keyword{
	background:#fdf0df;
	color:#fe8800;
	padding-top:16px;
}
/**********************/
#detailFooter{
	position:fixed;
	box-sizing: content-box;
	bottom:0;
	left:0;
	width:100%;
	padding:22px 0 19px;
	z-index:1;
	background:white;
	border-top:1px solid #a3a3a3;
}
.detail-footer-btn{
	width:50%;
	float:left;
}
.detail-footer-btn1 > div{
	padding:0 8px 0 19px;
}
.detail-footer-btn2 > div{
	padding:0 19px 0 8px;
}
#detailFooter.noOffline > .detail-footer-btn1{
	display:none;
}
#detailFooter.noOffline > .detail-footer-btn2{
	width: 72%;
	float: none;
	margin: 0 auto;
}
.detail-footer-btn .btn{
	width:100%;
	border-radius:4px;
	height:67px;
	line-height:67px;
	border:0;
	font-size:36px;
}
.detail-footer-btn1 .btn{
	background-color: #e1e1e1;
	color:#191919;
}
.detail-footer-btn2 .btn{
	background-color: #fe8800;
	color:#fff;
}
/**********************
#boxError{
	z-index:9;
	display:none;
	left:0;
	top:0;
	right:0;
	bottom:0;
	background:rgba(0, 0, 0, 0.4);
	z-index:8;
	position:fixed;
}
.boxError-box{
	width:500px;
	height:340px;
	top:50%;
	left:50%;
	margin-top:-170px;
	margin-left:-250px;
	background:white;
	position:fixed;
	padding:1em;
	border-radius:6px;
}
.boxError-box .multi-clm-box{
	margin:1em 0;
}
.boxError-box label{
	width:20%;
}
.boxError-box input,
.boxError-box textarea{
	width:100%;
}
.boxError-box textarea{
	height:130px;
}
.err-btm{
	text-align:center;
}
**********************/
@media(max-width:639px){
	#detail-top{
		padding:24px 20px 12px 20px;
	}
	#detail-top-upper{
		margin-bottom:10px;
	}
	.detail-poster{
		width:87px;
		height:120px;
	}
	.detail-score{
		height:15px;
		line-height:15px;
	}
	.detail-info{
		padding-top:1px;
		font-size:14px;
		line-height: 1em;
	}
	.detail-info > li{
		margin-bottom:11px;
	}
	#detailSourceBox{
		border-radius: 1px;
	}
	.detail-follow-btn{
		width:64px;
		height: 20px;
		line-height: 18px;
	}
	.detail-lk-report{
		font-size:13px;
	}
	.detail-lk-share{
		width:22px;
		height:22px;
	}

	/****************************/

	/**********************/
	#noProposal{
		min-height:148px;
	}
	/**********************/
	.review-general{
		padding:15px 15px 10px;
	}
	.review-score-avg{ 
		font-size: 13px;
		margin-right: 25px; 
	}
	.review-score-avg-tit{ 
		font-size: 11px;
	}
	.review-score-avg-body{ 
		padding:25px 0;
	}

	.review-score-detail{
		margin-top:2px;
		font-size:12px;
	}
	.review-score-detail-top{
		font-size:15px;
		margin-bottom:22px;
	}
	.review-score-detail-btm-more:before{
		top: -2px;
	}

	.review-btn{
		margin-bottom:16px;
	}
	.review-list{font-size: 11px;}
	.review-list li{padding: 11px 0;}
	.recContent{ margin:0 58px 0 15px; color:#757575;}
	.recContent > div{  margin-top: 9px;}
	.recContent > div:first-child{  margin-top: 0;}
	.recContent .appStars{ margin-right: 5px;}
	.recContent .model{ margin-right:6px;}
	.recContent .ver{ margin-left: 6px;}
	.recContent .time{float: right; width: 60px; overflow: hidden; height: 1em;}
	.review-list-desc{line-height: 1.5; color:#191919;}

	.icon-comment{ width: 29px; height: 29px; top:11px; right:14px; }
	.recSub{
		margin: 9px 9px 0;
		webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		-o-border-radius: 5px;
		border-radius: 5px;
	}
	.recSub li{ padding:11px 5px;}
	.recSubContent{ margin:0 49px 0 0px;}
	.recSubContent .model{ margin-left: 6px;}
	.recSub .icon-comment{ right: 4px;}
	.arrowTop,.arrowTopIn{border-width: 0 6px 6px 6px; top: -6px;  top:-6px; left: 15px;}
	.arrowTopIn{top: -5px;}

	/**********************/
	#synopsisCtn{
		padding:20px 15px;
		font-size:13px;
	}
	#synopsisCtn label{
		margin-top: 3px;
	}
	#synopsisCtn .keyword{
		padding: 5px 12px 8px 12px;
		margin-bottom: 12px;
	}
	#synopsisCtn a.keyword{
		padding-top:8px;
	}
	/**********************/
	#detailFooter{
		padding:11px 0 9px;
	}
	.detail-footer-btn1 > div{
		padding:0 4px 0 9px;
	}
	.detail-footer-btn2 > div{
		padding:0 9px 0 4px;
	}
	.detail-footer-btn .btn{
		border-radius:2px;
		height:33px;
		line-height:33px;
		font-size:18px;
	}
	/************************************/
