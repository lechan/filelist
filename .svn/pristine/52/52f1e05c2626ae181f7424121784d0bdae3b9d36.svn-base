		<script type="text/javascript">
        //page tracking
        var EVENT_ACTION='chnlDetail';
        window.Avatar= [];
        Avatar.push([document.querySelector("#btnFilter"), 'clickFilter']);
        Avatar.push([document.querySelector("#btnFilterCfm"), 'submitFilter']);
		Avatar.push([document.querySelector("#btnFilterCancel"), 'cancelFilter']);
		(function(){
			var scriptTag = document.createElement('script');
			scriptTag.type = 'text/javascript';
			scriptTag.async = true;
			scriptTag.src = 'js/avatarCfg.js';
			var t=document.getElementsByTagName("script")[0];t.parentNode.insertBefore(scriptTag,t);
		})()
        </script>
