<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>无标题文档</title>
<style type="text/css">
ul{ width:1000px; margin:0 auto;}
ul li{ display:block; line-height:30px; list-style:none; float:left; width:50px;}
</style>
<script type="text/javascript">
var cur_arr = ['1','2','3','4','5','6','7','8','9'];
var count = 3;

function C(n,m){
	if(n/2<m) m = n - m;
	var n1 = 1, n2 = 1;
	for(i=n,j=1;j<=m; n1 *= i--, n2 *= j++);
	return n1 / n2;	
}

console.log(C(9,3));

function list(arr,num){
	
	var html = '';
	var oUl = document.createElement('ul');
	document.body.appendChild(oUl);
	for(var i=0;i<arr.length;i++){
		var str = '';
		str += arr[i];
		
		for(var j=i+1;j<arr.length;j++){
			var str2 = ''
			str2 += (str+arr[j]);
			
			for(var k=j+1;k<arr.length;k++){
				var str3 = '';
				str3 += (str2+arr[k]);
				
				if(str3.length==num){
					html += '<li>'+str3+'</li>';	
				}else if(str3.length>num){
					break;	
				}else{
					continue;	
				}
				
				
			}
		}
	}
	oUl.innerHTML = html;
	
	console.log(oUl.getElementsByTagName('li').length);
	
}

window.onload = function(){
	list(cur_arr,count);	
}
</script>
</head>

<body>
</body>
</html>
