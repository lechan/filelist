<div class="mask" id="mask"></div>
<script  type="text/template" id="myorderTpl">
<li>
	<h2><%=way %></h2>
	<div>
		<label>出发时间：</label>
		<span><%=startim %></span>
	</div>
	<div>
		<label>购买人数：</label>
		<span><%=man %>人</span>
	</div>
	<div>
		<label>联系方式：</label>
		<span><%=mob %></span>
	</div>
	<div>
		<label>订单编码：</label>
		<span><%=ordercode %></span>
	</div>
	<div>
		<label>下单时间：</label>
		<span><%=tim %></span>
	</div>
	<div>
		<label>订单金额：</label>
		<strong><%=price %></strong>
	</div>
	<% if(payment == 0) { %>
		<h3><img src="images/icon5.png" width="75" /></h3>
	<% }else{ %>
		<h3><img src="images/icon6.png" width="75" /></h3>
	<% } %>
</li>
