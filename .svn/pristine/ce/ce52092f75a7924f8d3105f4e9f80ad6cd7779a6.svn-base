@charset "utf-8";
body,div,dl,dt,dd,h1,h2,h3,h4,h5,h6,form,input,textarea,p,button,span,i,ul,li,del,em{margin:0; padding:0; -webkit-tap-highlight-color:rgba(0,0,0,0);}
body{ font-family: "microsoft yahei"; font-size:22px; margin: 0; color: #000; line-height:1; background-color: #f2f2f2; min-width:320px;}
ul{list-style: none;}i,em{font-style: normal;}
a,a:active{text-decoration: none; color: #000;}
h2{ font-size: 16px; font-weight: normal;}
h3{ font-size: 14px; font-weight: normal;}
h4{ font-size: 12px; font-weight: normal;}
h5{ font-size: 12px; font-weight: normal;}
img{ border: none; vertical-align:top;}
.clearfix:after { visibility: hidden; display: block;font-size: 0; content: " "; clear: both;height: 0;}
.clearfix{*zoom:1;}
.pa{position:absolute}
.pf{position:fixed}
.tcenter{text-align:center}
.fl{ float:left;}
.fr{ float:right;}
.wrap{ max-width:480px; min-width:320px; margin:0 auto; overflow:hidden; background:#ededed; -webkit-transition:all ease-out 0.3s; position:relative; z-index:3;}
.detailWrap{ padding-bottom:50px;}
.score,
.userBtn li div,
.listWrap li h4 strong,
.listWrap li div.countdown i,
.more,
.listWrap li div.rank,
.beforeLogin h2
{ background:url(../images/icon.png) no-repeat; background-size:40px auto;}

.beforeLogin{ height:45px;}
.beforeLogin h2{ font-size:14px; color:#333; padding-left:20px; background-position:0px -174px; height:20px; line-height:20px; margin-top:14px;}
.beforeLogin .more{ margin-top:14px;}
.user{ padding:0px 15px; height:45px; background:#fff;}
.username{ font-size:14px; color:#333333; margin-top:8px;}
.username i{ font-size:10px; color:#fff; padding:0px 4px; font-style:italic; background:#F5A101; border-radius:2px; margin-right:5px;}
.score{ padding-left:18px; font-size:10px; color:#666666; height:13px; overflow:hidden; line-height:15px; margin-top:5px;}
.userBtn li{ width:30px; margin-left:15px; float:left;}
.userBtn li a{ display:block;}
.userBtn li div{ width:22px; height:22px; margin:4px auto 2px;}
.userBtn li h2{ font-size:10px; color:#808080; text-align:center;}
.userBtn li div.icon1{ background-position:0px -25px;}
.userBtn li div.icon2{ background-position:0px -47px;}
.userBtn li div.icon3{ background-position:0px -69px; position:relative;}
.notice{ width:10px; height:10px; border-radius:50%; background:#F06758; position:absolute; top:-3px; right:-4px;}
.lv{ width:100%; overflow:hidden; margin:7px 0px;}
.lv_left{float: left; width: 37.5%; -webkit-box-sizing: border-box; box-sizing: border-box;}
.lv_right{float: right; width: 62.5%; -webkit-box-sizing: border-box; box-sizing: border-box;}
.lv_left img{ width:100%;}
.lv_top{ -webkit-box-sizing: border-box; box-sizing: border-box; height:50%;}
.lv_top img{ width:100%;}
.lv_bottom{ height:50%;}
.lv_bottom_item{ width:50%; height:100%; float:left; -webkit-box-sizing: border-box; box-sizing: border-box;}
.lv_bottom_item img{ width:100%;}
.lv_border_left{ border-left:1px solid #fff;}
.lv_border_right{ border-right:1px solid #fff;}
.lv_border_top{ border-top:1px solid #fff;}
.lv_border_bottom{ border-bottom:1px solid #fff;}

.banner{ width:100%; position:relative; overflow:hidden; margin-bottom:7px;}
.banner_wrap{ width:100%; overflow:hidden; position:relative;}
.banner_wrap div{float:left; width:100%; position: relative; overflow:hidden;}
.banner_wrap div img{ width:100%;}
.banner_dot_wrap{ width:100%; overflow:hidden; position:absolute; bottom:10px; left:0px; text-align:center; height:5px;}
.banner_dot_wrap i{ width:5px; height:5px; border-radius:50%; overflow:hidden; font-size:0; background:#E7E7E7; display:inline-block; margin:0px 2px; vertical-align:top; -webkit-transition:all ease-out 0.2s;}
.banner_dot_wrap i.active{ background:#2cadf1;}

.topicItem{ width:100%; overflow:hidden; background:#fff; margin-bottom:7px;}
.topicItem h2{ border-bottom:1px solid #EAEAEA; height:35px; line-height:35px; padding:0px 10px; overflow:hidden;}
.topicItem span{ font-size:14px; color:#333333;}
.topicItem a{ font-family:"宋体"; font-size:20px;}
.listWrap{ overflow:hidden;}
.listWrap li{ width:33.3333%; float:left; border-right:1px solid #EAEAEA; padding:8px 0px 14px; -webkit-box-sizing:border-box; box-sizing:border-box;}
.listWrap li:nth-child(3n){ border:none;}

.listWrap li a{ width:100%; display:block;}
.listWrap li div.listImg{ text-align:center; margin-bottom:8px;}
.listWrap li div.listImg img{ width:87px; height:87px;}
.listWrap li h3{ padding:0px 10px; font-size:12px; line-height:15px; overflow:hidden; text-overflow:ellipsis; display:-webkit-box; -webkit-line-clamp:2;-webkit-box-orient:vertical; color:#333; height:30px; margin-bottom:6px;}
.listWrap li h4{ padding:0px 10px; font-size:12px; line-height:15px; margin-bottom:4px;}
.listWrap li h4 strong{ font-size:12px; padding-left:18px; background-position:0px -13px; color:#ff5a00; line-height:12px; overflow:hidden; display:inline-block; height:12px; vertical-align:top;}
.listWrap li h4 i{ margin-left:5px; text-decoration:line-through; color:#999; font-size:10px; line-height:10px; height:10px; display:inline-block; vertical-align:top; margin-top:2px;}
.listWrap li div.bottomTxt1{ padding:0px 10px; font-size:10px; color:#ff5a00;}
.listWrap li div.bottomTxt2{ padding:0px 10px; font-size:10px; color:#999;}
.listWrap li div.bottomTxt2 strong{ color:#f5a101;}
.listWrap li div.rank{ width:25px; height:24px; background-position:0px -92px; margin:-8px 0px 0px 10px; font-size:8px; color:#fff; line-height:20px; font-style:italic; text-align:center;}
.listWrap li div.countdown{ height:26px; text-align:center; font-size:14px; color:#333; margin-bottom:8px;}
.listWrap li div.countdown i{ width:26px; height:26px; line-height:26px; display:inline-block; text-align:center; margin:0px 2px; background-position:0px -116px; color:#666666;}
.tag{ background:#fff; margin-bottom:7px;}
.tag ul{ overflow:hidden;}
.tag ul li{ width:25%; float:left; border-bottom:1px solid #EAEAEA; border-right:1px solid #EAEAEA; -webkit-box-sizing:border-box; box-sizing:border-box;}
.tag ul li:nth-child(4n){  border-right:none;}
.tag ul li h2{ font-size:14px; color:#333333; text-align:center; margin:12px 0px 4px;}
.tag ul li div img{ width:62px; height:62px;}
.tag ul li div.listImg{ text-align:center; margin-bottom:8px;}
.more{ width:15px; height:20px; display:block; background-position:0px -142px; margin-top:8px;}
.arrow_down{ width: 0; height: 0; border-left: 4px solid transparent; border-right: 4px solid transparent;border-top: 4px solid #666;font-size: 0; line-height: 0; position:absolute;}
.arrow_up{width: 0;height: 0;border-left: 4px solid transparent; border-right: 4px solid transparent; border-bottom: 4px solid #666; font-size: 0; line-height: 0; position:absolute;} 
.pos1{ top:8px;}
.pos2{ top:5px;}
.pos3{ top:12px;}
.nav{ position:fixed; top:0px; max-width:480px; width:100%; height:40px; z-index:10; overflow:hidden;}
div.selected{ height:80px;}
.blankbox{ width:100%; height:40px;}
.nav ul{ width:100%; height:39px; border-bottom:1px solid #e0e0e0; background:#FAFAFA; overflow:hidden;}
.nav ul li{ width:25%; float:left; height:39px; line-height:35px; text-align:center;}
.nav ul li span{ font-size:14px; color:#666; position:relative;}
.nav ul li.active span{ color:#1499f7;}
.nav ul li.up .arrow_up{ border-bottom: 4px solid #1499f7;}
.nav ul li.down .arrow_down{ border-top: 4px solid #1499f7;}
.selectedItem{ width:100%; height:40px; overflow:hidden;}
.selectedItemWrap{ height:39px; border-bottom:1px solid #e0e0e0; background:#FAFAFA; overflow:hidden; padding:0px 20px;}
.selectedItemWrap span{ font-size:14px; color:#1499f7; padding:5px 8px; border:1px solid #1499f7; border-radius:5px; margin-top:5px; margin-right:10px; float:left;}
.selectedItemWrap span i{ margin-left:2px;}
.mainList{ background:#fff; width:100%;}
.mainList ul li{ border-bottom:1px solid #e0e0e0; padding:10px; overflow:hidden; position:relative;}
.mainList ul li a{ width:100%; display:block;}
.mainList ul li div.listImg{ float:left; margin-right:10px;}
.mainList ul li div.listImg img{ width:90px; height:90px;}
.mainList ul li h3{ font-size:14px; line-height:15px; overflow:hidden; text-overflow:ellipsis; display:-webkit-box; -webkit-line-clamp:2;-webkit-box-orient:vertical; color:#333; height:30px; margin-bottom:20px;}
.mainList ul li.lvItem{ position:relative;}
.mainList ul li.lvItem h3{ margin-bottom:10px;}
.mainList ul li.lvItem h4{ margin-bottom:2px;}
.lvIcon{ width:38px; height:16px; position:absolute; top:0px; left:0px; color:#fff; font-size:9px; padding:0px 4px; overflow:hidden; line-height:16px;}
.lvIcon i{ width:38px; height:16px; -webkit-transform:skewX(-15deg); transform:skewX(-15deg); background:#F78827; position:absolute; top:0px; left:-2px; z-index:1;}
.lvIcon span{ position:relative; z-index:2;}
.detailWrap .lvIcon{ width:50px; height:20px; line-height:20px; font-size:12px; display:none;}
.detailWrap .lvIcon i{ width:50px; height:20px; line-height:20px;}
.mainList ul li h4{ font-size:12px; line-height:15px; height:18px; overflow:hidden; margin-bottom:4px;}
.mainList ul li h4 strong{ font-size:15px; padding-left:20px; background:url(../images/icon.png) no-repeat; background-size:53px auto; background-position:0px -15px; color:#ff5a00;}
.mainList ul li h4 i{ margin-left:5px; text-decoration:line-through; color:#999; font-size:12px;}
.mainList ul li h5{ line-height:15px; color:#999; font-size:12px;}
.lvTips{ margin-bottom:2px; height:12px; overflow:hidden;}
.lvTips span{ font-size:8px; color:#ff4800; border:1px solid #FEDED1; background:#FEF3EE; padding:1px 2px; margin-right:5px; display:inline-block; height:8px; line-height:8px; vertical-align:top;}
.lvTips i{ font-size:8px; color:#999; display:inline-block; height:8px; line-height:8px; overflow:hidden; margin-top:2px; vertical-align:top;}
.lvTips i strong{ padding-left:10px; background:url(../images/icon.png) no-repeat; background-size:27px auto; background-position:0px 0px; font-weight:100;}
.nav_mask{ max-width:480px; width:100%; height:100%; position:absolute; top:0px; left:0px;}
.nav_tag{ max-width:480px; width:100%; height:100%; position:fixed; background:rgba(0,0,0,0.2); z-index:10; display:none;}
.nav_tag ul{ width:100%; overflow:hidden; background:#FEFEFE; position:absolute; z-index:2; padding:10px 0px;}
.nav_tag ul li{ width:25%; float:left; text-align:center;}
.nav_tag ul li span{ color:#666666; font-size:12px; border:1px solid transparent; padding:8px 13px; margin:10px 0px; display:inline-block;}
.nav_tag ul li.active span{ color:#1499f7; border:1px solid #1499f7; border-radius:5px;}
.nav_select{ max-width:480px; width:100%; height:100%; position:fixed; background:rgba(0,0,0,0.2); z-index:10; display:none;}
.selectWrap{ position:absolute; z-index:2; width:100%;}
.selectWrap h2{ width:100%; height:45px; line-height:45px; font-size:14px; text-align:center; background:#FEFEFE;}
.selectItem{ width:100%; background:#FEFEFE; padding-top:10px; border-bottom:1px solid #F2F2F2;}
.selectItem h3{ font-size:14px; color:#999999; padding:0px 10px;}
.selectItem ul{ width:100%; overflow:hidden; background:#FEFEFE;}
.selectItem ul li{ width:25%; float:left; text-align:center;}
.selectItem ul li span{ color:#666666; font-size:12px; border:1px solid transparent; padding:8px 13px; margin:10px 0px; display:inline-block;}
.selectItem ul li.active span{ color:#1499f7; border:1px solid #1499f7; border-radius:5px;}
.priceInput{ font-size:12px; color:#666; padding:10px 20px;}
.priceInput input{ width:50px; height:20px;}

.detailImg{ width:100%; position:relative; overflow:hidden; background:#fff; border-bottom:1px solid #eaeaea;}
.detailImgWrap{ width:9999px; overflow:hidden; position:relative;}
.detailImgWrap div{float:left; width:100%; position: relative; overflow:hidden; text-align:center;}
.detailImgWrap div img{ width:80%; margin:20px auto 10px;}
.detailImgDotWrap{ width:100%; overflow:hidden; text-align:center; height:10px; margin:10px 0px;}
.detailImgDotWrap i{ width:8px; height:8px; border:1px solid #d5d5d5; border-radius:50%; overflow:hidden; font-size:0; background:#fff; display:inline-block; margin:0px 4px; vertical-align:top; -webkit-transition:all ease-out 0.2s;}
.detailImgDotWrap i.active{ background:#1499f7; border:1px solid #d5d5d5;}
.merchInfo{ padding:12px 0px 10px; border-bottom:1px solid #eaeaea; background:#fff;}
.merchTitle{ overflow:hidden; margin-bottom:12px; padding:0px 15px;}
.merchTitle h1{ float:left; font-size:14px; color:#333; width:84%; line-height:16px; height:32px; overflow:hidden; text-overflow:ellipsis; display:-webkit-box; -webkit-line-clamp:2;-webkit-box-orient:vertical;}
.merchTitle div.collection{ width:10%; float:right; font-size:10px; color:#808080; border-left:1px solid #eaeaea; height:32px; padding-left:5px; text-align:right;}
.merchTitle div.collection i{ width:20px; height:20px; background:url(../images/icon.png) no-repeat; background-position:0px -42px; background-size:36px auto; display:block; float:right;}
.merchTitle div.collection i.active{ background-position:0px -173px;}
.merchTitle div.collection span{ display:block; line-height:15px; text-align:right; clear:both;}
.merchInfo h2{ font-size:18px; padding-left:25px; background:url(../images/icon.png) no-repeat; background-size:60px auto; background-position:0px -20px; color:#ff5a00; margin:0px 15px 10px;}
.merchInfo h2 span{ color:#FD5F19; font-size:12px; margin-left:10px;}
.lvInfo{ margin-bottom:5px; height:15px; overflow:hidden; padding:0px 15px;}
.lvInfo span{ font-size:10px; color:#fff; background:#FF5A00; padding:2px 3px; margin-right:5px; display:inline-block; height:10px; line-height:10px; vertical-align:top;}
.lvInfo i{ font-size:10px; color:#808080; display:inline-block; height:10px; line-height:10px; overflow:hidden; margin-top:2px; vertical-align:top; padding-right:5px;}
.lvInfo i strong{ padding-left:13px; background:url(../images/icon.png) no-repeat; background-size:32px auto; background-position:0px 2px; font-weight:100;}
.lvInfo b{ border-style: solid;border-width: 1px 1px 0 0; border-color:#999; display:inline-block;vertical-align:top;width:8px;height:8px; margin-left:5px;-webkit-transform: rotate(135deg);}
.lvInfo b.active{ -webkit-transform: rotate(-45deg); margin-top:5px;}
.lvScore{ background:#F5F5F5; display:none;}
.lvScore ul{ overflow:hidden; padding:5px 0px;}
.lvScore ul li{ width:33.33%; float:left; text-align:left; line-height:20px;}
.lvScore ul li span{ margin-left:10px; font-size:10px; color:#999999; padding:2px 4px;}
.lvScore ul li.active span{ border:1px solid #999;}
.lvScore ul li strong{ padding-left:13px; background:url(../images/icon.png) no-repeat; background-size:32px auto; background-position:0px 2px; font-weight:100; margin-right:5px;}
.merchInfo h3{ font-size:12px; color:#999; overflow:hidden; padding:5px 15px 0px;}
.merchInfo h3 span{ margin-right:10px; float:left; border-right:1px solid #eaeaea; display:inline-block; height:15px; line-height:15px; padding-right:10px;}
.merchInfo h3 span:nth-child(3){ border-right:0;}
.merchItem{ padding:12px 15px 10px; background:#fff; border-bottom:1px solid #eaeaea;}
.merchItem h2{ font-size:14px; color:#333; margin-bottom:6px;}
.merchDetail{ font-size:12px; color:#999; line-height:20px; overflow:hidden; text-overflow:ellipsis; display:-webkit-box; -webkit-line-clamp:5;-webkit-box-orient:vertical;}
.merchDetailActive{ display:block;}
.merchDetailMore{ text-align:right; font-size:12px; color:#999; margin-top:10px; height:15px; display:none;}
.merchDetailMore i{ border-style: solid;border-width: 1px 1px 0 0;display:inline-block;vertical-align:top;width:8px;height:8px; margin-left:5px;-webkit-transform: rotate(135deg);}
.merchDetailMore i.active{-webkit-transform: rotate(-45deg); margin-top:5px;}
.relativeRecommend{ overflow:hidden; padding-bottom:10px;}
.relativeRecommend li{ float:left;}
.relativeRecommend li a{ font-size:12px; color:#ff5a00; padding:5px 10px; background:#fef3ee; margin-right:10px;}
.customService{ font-size:12px; color:#999999;}
#exchangeBtn{ width:100%; height:48px; position:fixed; bottom:0px; left:0px;}
.exchangeBtn{ width:100%; height:48px; line-height:48px; color:#fff; background:#FE5B00; font-size:14px; text-align:center;}
.exchangeBtn span{ font-size:12px;}
.exchangeBtn span strong{ padding-left:13px; background:url(../images/icon.png) no-repeat; background-size:40px auto; background-position:0px -160px; font-weight:100; margin-left:2px;}
#mask{ width:100%; height:100%; z-index:100; top:0px; left:0px; background:rgba(25,25,25,0.6); display:none;}
#dialogWrapper{ width:264px; z-index:200; left:0px; top:0px; overflow:hidden; background:#F5F5F5; border-radius:3px;}
.pop_wrap{}
.pop_wrap h2{ font-size:15px; color:#191919; text-align:center; margin:23px 0px;}
.pop_wrap h3{ padding:0px 22px; margin-bottom:23px; font-size:13px; color:#666; line-height:20px;}
.pop_btn_wrap{ border-top:1px solid #eaeaea; height:48px; background:#FAFAFA;}
.pop_btn_item{ width:50%; height:48px; line-height:48px; font-size:14px; color:#333; text-align:center; float:left; -webkit-box-sizing:border-box; box-sizing:border-box;}
.pop_btn_item:nth-child(1){ border-right:1px solid #EAEAEA;}
#toast{bottom:-10em; font-size:12px; opacity:0; background:#000; color:#fff; padding:5px 10px; z-index:100; border-radius:5px; left:50%;}
/*animation & transition*/
.toastAnimation{-webkit-animation:toast 3s linear 1;}
@-webkit-keyframes toast{
	0%{opacity:0;}
	30%{opacity:0.7;}
	70%{opacity:0.7;}
	100%{opacity:0;}
}