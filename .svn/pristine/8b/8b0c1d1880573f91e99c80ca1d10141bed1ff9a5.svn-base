.main_nav_list li.news_btn span{ background-position:0px 0px;}
.main_nav_list li.search_btn span{ background-position:0px -25px;}
.main_nav_list li.collect_btn span{ background-position:0px -50px;}
.main_nav_list li.push_btn span{ background-position:0px -75px;}
.main_nav_list li.history_btn span{ background-position:0px -100px;}
.main_nav_list li.set_btn span{ background-position:0px -125px;}
.main_nav_list li.quit_btn span{ background-position:0px -150px;}
.nav_mask{ width:100%; height:100%; position:absolute; top:0px; left:0px; z-index:2; display:none;}

.btn_grey{ padding:5px 10px; font-size:14px; border-radius:5px; background:#f9f9f9; border:1px solid #999999;}
.btn_red{ padding:5px 10px; font-size:14px; border-radius:5px; background:#f49d31; border:1px solid #f49d31; color:#fff;}
.page_item{ position:absolute; top:0px; left:0px; width:100%; display:none; overflow:hidden; background:#f6f6f6; height:100%; -webkit-transition:left 100ms ease-out;}
.page_header{ width:100%; height:42px; background:url(../images/headerbg.png) repeat-x;}
.header_left,.header_right{ width:30%; height:100%; float:left;}
.header_right{ text-align:right; line-height:40px;}
.header_title{ text-align:center; width:40%; height:100%; float:left; font-size:20px; color:#fff; line-height:42px;}
.nav_btn,.logo,.back_btn,.topic_list_btn i{ background-image:url(../images/main.png); background-repeat:no-repeat;}
.nav_btn{ width:23px; height:24px; background-position:-40px 0px; float:left; margin:10px;}
.back_btn{ width:29px; height:24px; background-position:-63px 0px; float:left; margin:10px;}
.share_btn{ padding:8px 20px; background:#fff; border-radius:5px; font-size:14px; color:#333; margin:10px;}
.logo{ width:40px; height:32px; background-position:0px 0px; display:block; margin:4px auto;}
.page_nav{ width:100%; height:40px; background:#efefef; border-bottom:1px solid #cccccc; position:relative;}
.nav_wrap{ width:90%; height:40px; color: #333333; text-align: center; position:relative;}
.page_sub_nav{ width:100%; height:30px; background:#efefef; position:relative;}
.sub_nav_wrap{ width:100%; height:30px; color:#333; text-align:center; position:relative;}
.bar {position: absolute;height: 6px;bottom: 0;overflow: hidden;-webkit-box-sizing: border-box;-webkit-transition: left 200ms ease-out;left:0px; width: 72px;background:url(../images/arrow.png) no-repeat center top; border-bottom:2px solid #3994e0;}
