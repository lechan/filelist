.history_tab li.active{ color:#4B82CC; border-bottom:2px solid #4B82CC; background:url(../images/arrow.png) no-repeat center bottom;}

/**
 *
 * Pull down styles
 *
 */
.pullDown, .pullUp, .flip, .loading{
	text-align:center;
	background:#fff;
	height:40px;
	line-height:40px;
	padding:5px 10px;
	border-bottom:1px solid #ccc;
	font-weight:bold;
	font-size:14px;
	color:#888;
}
.pullDownIcon, .pullUpIcon  {
	display:inline-block;
	width:30px; height:30px;
	margin-top:5px;
	background:url(../images/pull-icon@2x.png) 0 0 no-repeat;
	-webkit-background-size:30px auto; background-size:30px auto;
	-webkit-transition-property:-webkit-transform;
	-webkit-transition-duration:250ms;	
}
.pullDownLabel,.pullUpLabel{ display:inline-block; height:40px; line-height:40px; font-size:12px; color:#333; vertical-align:top; margin-left:5px; font-weight:100;}
.pullDownIcon {
	-webkit-transform:rotate(0deg) translateZ(0);
}
.pullUpIcon  {
	-webkit-transform:rotate(-180deg) translateZ(0);
}

.flip .pullDownIcon {
	-webkit-transform:rotate(-180deg) translateZ(0);
}

.flip .pullUpIcon {
	-webkit-transform:rotate(0deg) translateZ(0);
}

.loading .pullDownIcon,.loading .pullUpIcon {
	background-position:0 100%;
	-webkit-transform:rotate(0deg) translateZ(0);
	-webkit-transition-duration:0ms;

	-webkit-animation-name:loading;
	-webkit-animation-duration:2s;
	-webkit-animation-iteration-count:infinite;
	-webkit-animation-timing-function:linear;
}

@-webkit-keyframes loading {
	from { -webkit-transform:rotate(0deg) translateZ(0); }
	to { -webkit-transform:rotate(360deg) translateZ(0); }
}

.pop{ width:100%; height:100%; position:absolute; top:0px; left:0px; background:rgba(0,0,0,0.4); z-index:5; display:none;}
.pop dl{ width:300px; position:absolute; left:50%; margin-left:-150px; bottom:60px; background:#F5F5F5; text-align:center; line-height:40px; border-radius:5px;}
.pop dl dt{ height:40px; font-size:12px; color:#333;}
.pop dl dd{ height:40px; color:#3368c5; font-size:16px; border-top:1px solid #ccc;}
.pop dl dd.pop_current{ color:#e4775c;}
.close_pop{ width:300px; position:absolute; left:50%; margin-left:-150px; bottom:10px; background:#F5F5F5; color:#3368c5; font-size:16px; text-align:center; height:40px; line-height:40px; border-radius:5px;}